[t:0x0407,n:text]
{
    // Standard hireable convs
    [0x0000]
    {
        from = "Pff then not!";
        to = "Na dann halt nicht!";
    }
    [0x0000]
    {
        from = "Yay let's go!";
        to = "Auf geht's!";
    }
    [0x0000]
    {
        from = "Wait, what?";
        to = "Bitte was?";
    }
    [0x0000]
    {
        from = "No hard feelings mate... Wanna go together again?";
        to = "Nichts für ungut Kumpel... Sollen wir wieder zusammen gehen?";
    }

    // ARANNA

    // Planet
    // Holzfäller: Sie/Sich
    [0x0000]
    {
        from = "Welcome to Little Iliyara Island!
Glad to see you arrived well. It must have been a long and tiresome journey through the vast stretches of Central Aranna...";
        to = "Willkommen auf der Insel Klein-Iliyara!
Freut mich, dass Sie gut angekommen sind. Es muss eine weite und beschwerliche Reise durch die Weiten Zentralarannas gewesen sein...";
    }
    [0x0000]
    {
        from = "I am a loggerman and I live here as a hermit. Not many people visit here - most people who do come to Lake Chalice visit Main Iliyara Island to see the Acropolis and the Palace of Night; few can be bothered to visit the other small islands around it.
I get visitors about once or twice a year, they usually want to study the ruins here, and to be honest the line between scholar and nuthead is often blurry, no offense meant.";
        to = "Ich bin Holzfäller und lebe hier als Einsiedler. Nicht viele Leute kommen hier her - die meisten, die an den Kelchsee kommen, besuchen die Hauptinsel Iliyara, um die Akropolis und den Palast der Nacht zu sehen; nur wenige machen sich die Mühe, die anderen kleinen Inseln drumherum zu besuchen.
Ich habe hier ein-zweimal im Jahr Besucher, die für gewöhnlich die Ruinen hier studieren wollen, und um ehrlich zu sein verläuft die Grenze zwischen Wissenschaftlern und Wirrköpfen oft fließend, nichts für ungut.";
    }
    [0x0000]
    {
        from = "As I wrote to you in my letter, your aunt was staying here, and after a week she suddenly disappeared. She hasn't returned in the meantime either, and I have still no clue as to where she's gone. I must've searched the island half a dozen times by now.";
        to = "Als ich Ihnen meinen Brief schrieb, war Ihre Tante hier zu Gast, und nach einer Woche verschwand sie plötzlich. Sie ist inzwischen auch nicht wieder aufgetaucht, und ich habe immer noch keinen Schimmer wo sie sein könnte. Ich habe die Insel inzwischen wohl ein halbes Dutzend mal durchkämmt.";
    }
    [0x0000]
    {
        from = "In my house is still her chest with her belongings. If she remains disappeared, it belongs to you now, I guess.";
        to = "In meinem Haus ist noch ihre Truhe mit ihrer Habe. Wenn sie verschollen bleibt, gehört es dann wohl Ihnen, nehme ich an.";
    }
    [0x0000]
    {
        from = "Thank you for coming. I hope you can find her, or at least find out what happened to your aunt...";
        to = "Danke, dass Sie gekommen sind. Ich hoffe Sie können sie finden, oder zumindest herausfinden was mit Ihrer Tante passiert ist...";
    }
    [0x0000]
    {
        from = "You're back! I'm so glad to see you alive... But how you have changed! And where have you been?";
        to = "Sie sind zurück! Ich bin so froh Sie am Leben zu sehen... Aber wie Sie sich verändert haben! Und wo waren Sie?";
    }

    // Moon
    // Magische Mondzwerge: du/dich
    [0x0000]
    {
        from = "Hello, stranger, welcome to the moon!
We are the Magic Moon Dwarves and we live up here.";
        to = "Hallo, Fremdling, willkommen auf dem Mond!
Wir sind die Magischen Mondzwerge und wir wohnen hier oben.";
    }
    [0x0000]
    {
        from = "Do visit our tavern, it's right up north from here!";
        to = "Besuche unbedingt unsere Taverne, sie ist geradewegs nördlich von hier!";
    }
    [0x0000]
    {
        from = "Enjoy your time on the moon!";
        to = "Genieße die Zeit auf dem Mond!";
    }
    [0x0000]
    {
        from = "Welcome to the moon! Have you visited our Tavern yet?";
        to = "Willkommen auf dem Mond! Hast du schon unsere Taverne besucht?";
    }
    [0x0000]
    {
        from = "It's a beautiful weather today, isn't it?";
        to = "Wunderschönes Wetter heute, nicht wahr?";
    }
    [0x0000]
    {
        from = "How do you like the moon? You should see it in winter!";
        to = "Wie gefällt dir der Mond? Du solltest ihn im Winter sehen!";
    }
    [0x0000]
    {
        from = "Welcome, stranger, welcome to the Magic Moon Tavern!
Would you like to eat or drink something? On the menu is mulled mead and gremal stew.";
        to = "Willkommen, Fremdling, willkommen in der Magischen Mondtaverne!
Hättest du gern was zum Essen oder zum Trinken? Auf der Karte sind Glüh-Met und Gremal-Eintopf.";
    }
    [0x0000]
    {
        from = "Hello, welcome to the moon! Would you like to have some gremal stew?";
        to = "Hallo, willkommen auf dem Mond! Möchtest du etwas Gremal-Eintopf?";
    }
    [0x0000]
    {
        from = "How do you like our tavern? We all live here. Yes, right in the tavern. That's cool ah?";
        to = "Wie gefällt dir unsere Taverne? Wir alle wohnen hier. Ja, direkt in der Taverne. Cool was?";
    }
    [0x0000]
    {
        from = "Try the gremal stew here, it's delicious!";
        to = "Versuch den Gremal-Eintopf hier, er ist köstlich!";
    }
    [0x0000]
    {
        from = "Have some mulled mead, it's very good here and not too sweet.";
        to = "Trink etwas Glüh-Met, er ist sehr gut hier und auch nicht zu süß.";
    }
    [0x0000]
    {
        from = "The other dwarves are out camping.";
        to = "Die anderen Zwerge sind draußen am Campen.";
    }

    // Red Planet
    // Kleine grüne Männchen: du/dich
    [0x0000]
    {
        from = "Welcome on the Red Planet, humans! It's been a long time our world hasn't seen your kind...";
        to = "Willkommen auf dem Roten Planeten, Menschen! Euerart hat unsere Welt eine lange Zeit nicht gesehen...";
    }
    [0x0000]
    {
        from = "Come visit our town and meet all the friendly little green men living here. In particular you should visit our townhall, where Mayor Neener lives. He'll also try to sell you equipment, tho, so there's that.";
        to = "Komm unser Städtchen besuchen und lern die ganzen netten kleinen grünen Männchen kennen, die hier leben. Besonders das Rathaus solltest du besuchen, wo Bürgermeister Neener wohnt. Er wird auch versuchen dir Ausrüstung zu verkaufen, aber naja.";
    }
    [0x0000]
    {
        from = "And if you have a little time, definitely have a walk in the canyon. It's beautiful down there - much greener than up here, of course.";
        to = "Und wenn du ein bisschen Zeit hast, mach auf jeden Fall einen Spaziergang in der Schlucht. Es ist wunderschön da unten - viel grüner als hier oben, natürlich.";
    }
    [0x0000]
    {
        from = "Again welcome, and enjoy your stay!";
        to = "Abermals willkommen, und einen schönen Aufenthalt!";
    }
    [0x0000]
    {
        from = "It is my pleasure and honour to welcome you to Red Planet. Your kind has not set foot on this world for generations - This might be the beginning of a grand new era of friendship and cooperation between our worlds. Not to mention my village might become a major hub of commerce soon!";
        to = "Es ist mir ein Vergnügen und eine Ehre, dich auf dem Roten Planeten willkommen zu heißen. Euerart hat seit Generationen keinen Fuß auf diese Welt gesetzt - Dies könnte der Anfang einer großartigen neuen Ära von Freundschaft und Zusammenarbeit zwischen unseren Welten sein. Nicht zu erwähnen dass mein Dorf bald ein zentraler Handelsknotenpunkt werden könnte!";
    }
    [0x0000]
    {
        from = "As I'm sure you know, our Red Planet is a very old world, with a hundred millennia of history, and is currently breathing its last. Having a weaker magnetic field than your own home, the sun has blown most of our atmosphere out into the void. Already the surface is dead and uninhabitable, and it is only in deep crevices like this one that little green men still roam. But no reason to be sad - nothing in the universe is made for eternity.
Anyway, I recommend you to have a walk down in the canyon and enjoy the lush scenery. There are a few wild beasts roaming but nothing to be afraid of.";
        to = "Wie du sicherlich weißt, ist unser Roter Planet eine sehr alte Welt, die hundert Jahrtausenden Geschichte hinter sich hat, und in ihren letzten Zügen liegt. Dadurch, dass er ein schwächeres Magnetfeld als deine Heimatwelt hat, hat die Sonne den Großteil unserer Atmosphäre bereits in die Leere hinausgeblasen. Schon jetzt ist die Oberfläche tot und unbewohnbar, und nur in tiefen Furchen wie dieser hier leben noch kleine grüne Männchen. Aber kein Grund Trübsal zu blasen - nichts im Universum ist für die Ewigkeit gemacht.
Wie dem auch sei, ich empfehle dir, einen Spaziergang in der Schlucht zu machen und die üppige Landschaft zu bewundern. Es laufen ein paar Wildtiere herum, aber nichts vor dem man sich fürchten müsste.";
    }
    [0x0000]
    {
        from = "Speaking of commerce, I have some things on sale that might interest you. Want to have a look?";
        to = "A propos Handel, ich habe hier ein paar Dinge im Angebot die dich interessieren könnten. Magst du mal schauen?";
    }
    [0x0000]
    {
        from = "Howdy, human! How do you like our canyon? 'S the most beautiful canyon on the Red Planet, I tell ya. Just be a wee bit careful with them little lungers - they're usually quite harmless, but there's a real nasty one right over there!";
        to = "Hallöchen, Mensch! Na, wie gefallt dir unsere Schlucht? Ist die schönste Schlucht auf dem ganzen Roten Planeten, sag ich dir. Sei nur es kliis Bitzeli vorsichtig mit denne Springer - die sind normal völlig harmlos, oder, aber da drüben sitzt ein echter fieser!";
    }
    [0x0000]
    {
        from = "Welcome to the Red Planet! Enjoy your stay in the canyon.";
        to = "Willkommen auf dem Roten Planeten! Einen schönen Aufenthalt in der Schlucht.";
    }
    [0x0000]
    {
        from = "Beep, boop! We come in peace! Bring me to your leader! Just joking, haha.";
        to = "Piep, piep! Wir kommen in Frieden! Bring mich zu deinem Anführer! War nur Spaß, haha.";
    }
    [0x0000]
    {
        from = "Greetings, bigfoot! Have you seen the canyon already? Nice walk down there, isn't it? You know, the atmosphere of our world is slowly dwindling, so the last inhabitable places are canyons like this one. But no hurry, there'll be enough air to breathe for a couple millennia more.
Yeah, it's a dying world. So romantic, right?";
        to = "Grüße, Großfuß! Hast du schon die Schlucht gesehen? Schön zum spazieren da unten, oder? Weißt du, die Atmosphäre unserer Welt wird langsam dünner, daher sind Schluchten wie diese die letzten bewohnbaren Orte. Aber keine Eile, wir werden noch ein paar Jahrtausende länger Luft zum Atmen haben.
Ja, es ist eine sterbende Welt. Voll romantisch, gell?";
    }

    // ALPHA CENTAURI

    // Eddiloo: Ihr/Euch, außer dem Maultierverkäufer
    [0x0000]
    {
        from = "Did you really just come from the stars? How incredible!
As the mayor of this village I want to welcome you to Eddiloo... and Alpha Centauri, I guess.";
        to = "Seid Ihr gerade tatsächlich von den Sternen gekommen? Das ist ja unglaublich!
Als die Bürgermeisterin dieses Dorfes möchte ich Euch sehr herzlich in Eddiloo willkommen heißen... und auf Alpha Centauri, schätze ich.";
    }
    [0x0000]
    {
        from = "You are looking for an ancient artifact with a hammer or fish symbol on it? Not sure I can help you...
Maybe a good place to go looking for ancient artifacts would be the Crypt... yes. It's right in the forest. Be careful though - it's a New Moon, and the woods are full of Krugs, and in the Crypt, the dead might be... awake.";
        to = "Ihr sucht nach einem antiken Artefakt mit einem Hammer- oder Fischsymbol darauf? Bin mir nicht sicher ob ich Euch helfen kann...
Ein guter Ort um nach antiken Artefakten zu suchen wäre vielleicht die Gruft... ja. Sie liegt geradewegs im Wald. Aber seit auf der Hut - es ist Neumond, und die Wälder sind voll der Krugs, und die Toten in der Gruft könnten... wach sein.";
    }
    [0x0000]
    {
        from = "Apart from ancient artifacts, is there anything I can help you with?";
        to = "Abgesehen von antiken Artefakten, kann ich irgendetwas für Euch tun?";
    }
    [0x0000]
    {
        from = "Hello stranger, welcome to Alpha Centauri.
Do you happen to be interested in a packmule? Will come in handy when you go out and find more treasure than you can carry...";
        to = "Hallo Fremder, willkommen auf Alpha Centauri.
Wärst du zufällig an einem Maultier interessiert? Praktisch, wenn du da draußen herumläufst und mehr Schätze findest als du tragen kannst...";
    }
    [0x0000]
    {
        from = "I hope Eberhardt is doing well?";
        to = "Ich hoffe Eberhardt geht es gut?";
    }
    [0x0000]
    {
        from = "Is it true you came from the stars? Oh please take me with you! I have a few debts to pay but then I'm all yours. Take me away from here, will you? This is the middle of nowhere. There's not a single tavern within a week's march! I want to see the world and have adventures...";
        to = "Ist es wahr, dass Ihr von den Sternen kommt? Oh bitte nehmt mich mit! Ich muss ein paar Schulden begleichen aber dann gehöre ich ganz Euch. Werdet Ihr mich mitnehmen? Hier sagen sich Fuchs und Hase gute Nacht. Keine Taverne im Umkreis von sieben Tagesmärschen! Ich will die Welt sehen und Abenteuer erleben...";
    }

    // VEGA

    // Weiser Tretch: du/dich
    [0x0000]
    {
        from = "Ooooh! Aaaah! I am the Wise Tretch! Bow down in--

Hold on, you're not a Gulik, are you?";
        to = "Ooooh! Aaaah! Ich bin der Weise Tretch! Verneigt euch in--

Warte, du bist gar kein Gulik, oder?";
    }
    [0x0000]
    {
        from = "You come from the stars? Well that sounds like a big heap of nonsense. But that's not much surprising. You animals have no roots, that's your problem!";
        to = "Du kommst von den Sternen? Selten so einen Blödsinn gehört. Aber das überrascht mich nicht. Ihr Tiere habt keine Wurzeln, das ist euer Problem!";
    }
    [0x0000]
    {
        from = "So you're not a Gulik, that much's for sure. Also no Hassat, though my memory may fail me; I haven't seen a Hassat in these parts since I was a little sapling. Here are only two tribes of Gulik permanently at war with each other, as is the way of animals, and the occasional Braak coming down from their nests in the north...";
        to = "Also ein Gulik bist du nicht, soviel steht fest. Auch kein Hassati, wobei mir die Erinnerung einen Streich spielen kann; ich habe keine Hassati in der Gegend gesehen seit ich ein kleiner Schössling war. Hier gibt es nur zwei Stämme von Guliks die sich ständig bekriegen, wie es Tiere nun mal tun, und gelegentlich einige Braak die aus ihren Nestern im Norden hier herunterwandern...";
    }
    [0x0000]
    {
        from = "You want to walk around? Well, I can only advise against it. The world is a big place one can easily get lost in. I hear all about it when the Guliks come, from one tribe or the other, and seek advice from the Wise Tretch, about mating, or hunting, or conquering the opposing tribe. So listen closely now, while I tell you all I know about the forest, and make sure to remember it all, so you will find the way back.";
        to = "Du willst herumlaufen? Nun, ich kann nur davon abraten. Die Welt ist groß und man kann leicht darin verloren gehen. Ich höre alles darüber wenn die Guliks kommen, vom einen Stamm oder dem anderen, und den Rat des Weisen Tretch suchen, ob zur Paarung, oder zur Jagd, oder zur Eroberung des anderen Stammes. Also hör gut zu, ich erzähle dir alles, was ich über diesen Wald weiß, und merke es dir, damit du auch den Rückweg findest.";
    }
    [0x0000]
    {
        from = "One of the Gulik tribes lives to the southwest from here. They live on a little plateau around a giant tree, and nearby are ruins of two giant feet that look like yours, built of stone. So if you get lost and find two giant feet of stone, go northeast!

South of here, there is a rift in the ground. It starts just south of here and continues towards the east. So if you get lost and find a rift, walk to its west end and then turn north... you get the idea.
At the eastern end of that rift is a bright green meadow, and east of that there's another giant tree somewhere. And if you're stupid enough to walk even further, far in the east from here there's reportedly a lake.

The most interesting part is probably the north/northeast. But be careful, there's not only the other tribe of Guliks, but also dangerous beasts like Mantraps, and in the far north there live the Braak.
Basically straight northeast from here is a place where two giant trees grow on both sides of a river. Past that is the Gulik camp. North from the Gulik camp is another lake, this one has Syrrus in it. West from the Syrrus lake is a larger field of ruins, and further northwest from there, basically straight north from here, there are two Braak nests.
So if you find two giant trees, or a Syrrus lake, or a field of ruins, turn southwest to find back; and if you find the Braak, go straight south.

Good luck and happy hunting! Hope to see you again!";
        to = "Der eine Gulik-Stamm lebt von hier aus im Südwesten. Sie leben auf einem kleinen Plateau um einen Riesenbaum, und daneben sind Ruinen zweier riesiger Füße die aussehen wie deine, aus Stein gebaut. Also wenn du den Weg verlierst und zwei riesige Steinfüße findest, geh in Richtung Nordosten!

Im Süden befindet sich eine Kluft. Sie fängt gerade südlich von hier an und verläuft in Richtung Osten. Also wenn du den Weg verlierst und eine Kluft findest, folge ihr nach Westen bis zum Ende und geh dann nach Norden... du verstehst schon.
Am östlichen Ende der Kluft liegt eine helle grüne Aue, und östlich davon wächst irgendwo ein weiterer Riesenbaum. Und wenn du dumm genug bist sogar noch weiter zu laufen, liegt weit im Osten von hier ein See.

Die interessanteste Gegend ist vermutlich der Norden/Nordosten. Aber sei vorsichtig, dort lebt nicht nur der andere Gulik-Stamm, sondern auch gefährliche Biester wie die Menschenfalle, und weit im Norden leben die Braak.
Im Grunde geradeaus nordwestlich von hier liegt ein Ort, wo zwei Riesenbäume zu beiden Seiten eines Flusses wachsen. Dahinter liegt das Gulik-Camp. Nördlich vom Gulik-Camp befindet sich ein weiterer See, mit Syrrus darin. Westlich von dem Syrrus-See ist ein größeres Ruinenfeld, und von dort aus weiter nordwestlich, von hier aus geradewegs nördlich, befinden sich zwei Braak-Nester.
Also wenn du zwei Riesenbäume findest, oder einen See voller Syrrus, oder ein Ruinenfeld, wende dich nach Südwesten um hierher zurückzufinden; und wenn du auf Braak triffst, gehe geradeaus nach Süden.

Viel Glück und gute Jagd! Ich hoffe ich sehe dich wieder!";
    }

    // BETELGEUSE
    // Uträische Hippies: du/dich; Ältester vom See Du/Dich

    // First town
    [0x0000]
    {
        from = "Blessings, Stranger. Welcome to Betelgeuse.";
        to = "Segen, Fremdling. Willkommen auf Beteigeuze.";
    }
    [0x0000]
    {
        from = "You must be the sibling child of the Lady Luna. She half expected you to come.";
        to = "Du musst das Geschwisterkind der Dame Luna sein. Sie erwartete halb deine Ankunft.";
    }
    [0x0000]
    {
        from = "The Lady Luna is with the others at the tents. It's our little tent village, we call it Palim Palim.";
        to = "Die Dame Luna ist bei den anderen an den Zelten. Es ist unser kleines Zeltlager, wir nennen es Palim Palim.";
    }
    [0x0000]
    {
        from = "Be our guest!";
        to = "Sei unser Gast!";
    }
    [0x0000]
    {
        from = "What took you so long?";
        to = "Wo warst du so lang?";
    }
    [0x0000]
    {
        from = "I'm actually so happy to see you, my dearest!
See what I discovered - the ancient Empire of Stars had contact with Beings from other actual stars! Extraterrestrials! It was an interstellar empire spanning across a whole eight star systems! It's truly incredible!";
        to = "Ach, ich bin so froh dich zu sehen, mein Liebstes!
Sieh was ich entdeckt habe - das antike Reich der Sterne stand im Kontakt mit Wesen von richtigen anderen Sternen! Außerirdische! Es war ein interstellares Reich, das sich über ganze acht Sternensysteme erstreckte! Es ist absolut unglaublich!";
    }
    [0x0000]
    {
        from = "What do you mean, you want to go HOME of all places?
...Alright let's go then... But we need to find an Aranna Starstone first...";
        to = "Was meinst du, du willst NACH HAUSE ausgerechnet?
...Okay dann gehen wir... Aber zuerst müssen wir so einen Aranna-Sternstein finden...";
    }
    [0x0000]
    {
        from = "Blessings, friend.
If you are looking for ancient artifacts similar to yours, you should see the Elder of the Lake about them. He lives over in a place called Wishy-Washy, at the Great Lake to the East.
Greet him from me!";
        to = "Segen, Freund.
Wenn du nach antiken Artefakten ähnlich den deinen suchst, solltest du den Ältesten vom See um Rat fragen. Er lebt drüben in einem Ort namens Wischi-Waschi, am Großen See im Osten.
Grüß ihn von mir!";
    }
    [0x0000]
    {
        from = "The Elder gave me this stone, long ago, to guard it. I just received a telepathic vision from him telling me to give it to you. The Elder is wise; and I can sense that it will be in good hands.";
        to = "Der Älteste gab mir diesen Stein, vor langer Zeit, ihn zu bewahren. Ich habe soeben eine telepathische Vision von ihm empfangen, die mich hieß, ihn dir zu überreichen. Der Älteste ist weise; und ich kann spüren, dass der Stein in guten Händen sein wird.";
    }
    [0x0000]
    {
        from = "I pray for your safe and happy return home.
And whenever you want, come visit us, you'll always be welcome here!
Go with the blessings of the Great Mother.";
        to = "Ich bete für deine sichere und glückliche Heimkehr.
Und komm uns besuchen wann immer du magst, du wirst hier immer willkommen sein!
Geh mit dem Segen der Großen Mutter.";
    }
    [0x0000]
    {
        from = "Blessings to you!
So you come from the stars? Dope.
You know, this place is a paradise for us. Except if you're afraid of spiders, I guess...
Unrelatedly, ...";
        to = "Segen scheine auf dich!
Du kommst also von den Sternen? Geil.
Weißt du, dieser Ort ist ein Paradies für uns. Außer wenn man sich vor Spinnen fürchtet, schätze ich...
Anderes Thema, ...";
    }
    [0x0000]
    {
        from = "Would you like me to join you? I'd love to see the stars and stuff... I'm not bad with a bow, which might come in handy. So what do you think?";
        to = "Soll ich mich euch anschließen? Ich würde sehr gerne die Sterne sehen und so... Ich kann ganz gut mit einem Bogen umgehen, das könnte gelegen kommen. Also, was meinst du?";
    }
    [0x0000]
    {
        from = "Blessings, my friend.
I have a few things here that might be useful for you. Come have a look!";
        to = "Segen, mein Freund.
Ich habe ein paar Dinge hier die dir nützlich sein könnten. Komm und seh!";
    }
    [0x0000]
    {
        from = "Blessings, stranger, welcome to Betelgeuse.
Do you happen to be interested in a pack animal? Will come in handy when you go out and find more treasure than you can carry...
I can spare good Dingaling here. She's well-behaved and also brave, she'll defend you and follow you wherever you go.";
        to = "Segen, Fremdling, willkommen auf Beteigeuze.
Wärst du an einem Packtier interessiert? Sowas ist immer praktisch wenn man draußen herumläuft und mehr Schätze findet als man tragen kann...
Ich kann die gute Dingaling hier entbehren. Sie ist brav und auch mutig, sie wird dich verteidigen und dir folgen wohin auch immer du gehst.";
    }
    [0x0000]
    {
        from = "I hope Dingaling is doing well?
Sorry, we need the other pack animals ourselves.";
        to = "Ich hoffe Dingaling geht es gut?
Verzeih, wir benötigen die anderen Packtiere selbst.";
    }
    [0x0000]
    {
        from = "How do you like our Beetles? They're beautiful, aren't they?
We keep them for the delicious Beetlejuice, of course. Fancy a glass?";
        to = "Wie gefallen dir unsere Beetles? Sie sind wunderschön, hab ich Recht?
Wir halten sie natürlich für den leckeren Beetlejuice. Möchtest du ein Glas?";
    }
    [0x0000]
    {
        from = "Blessings, stranger from another star.
So how do you like this planet? Is it much different from yours?
One thing that might be different is our sunlight - Betelgeuse is a pink star. Looks quite pretty ah? I imagine yellow sunlight to look a bit agressive... But to each their own, I guess.
Anyways hope you like it here!";
        to = "Segen, Fremdling von einem anderen Stern.
Also, wie gefällt dir dieser Planet? Ist er sehr anders als dein eigener?
Eine Sache, die vielleicht anders ist, ist unser Sonnenlicht - Beteigeuze ist ein pinker Stern. Sieht recht hübsch aus was? Ich stelle mir gelbes Sonnenlicht etwas aggressiv vor... Aber jedem das Seine, nehme ich an.
Hoffe jedenfalls dass es dir hier gefällt!";
    }

    // Lake town
    [0x0000]
    {
        from = "Welcome, youngling. May the blessings of the Great Mother never stop shining upon you.
I sense a great fate on you. No surprise for someone who discovered the old way to the stars, of course.
I also sense that I am destined to give this artifact here to you. May it guide you in your endeavors, and may you use it to mend what has been broken for a millennium.";
        to = "Willkommen, Jüngling. Möge der Segen der Großen Mutter nie aufhören auf Dich zu scheinen.
Ich spüre ein großes Schicksal auf Dir. Nicht überraschend für jemanden, der den alten Weg zu den Sternen gefunden hat, natürlich.
Ich spüre ebenfalls, dass es mir bestimmt ist, Dir dieses Artefakt hier zu geben. Möge es Dich in Deinen Unternehmungen leiten, und mögest Du es nutzen, zu erneuern, was seit einem Millennium verloren ist.";
    }
    [0x0000]
    {
        from = "Our history books tell about the ancient empires. The Utraean Empire and the Empire of Stars were neighbors, and in many ways like siblings - at times allies, at other times rivals. In the end they both crumbled, of course.
Nobody knows how it came that Utraeans live on this planet now - it belonged to the Empire of Stars, of course, not the Utraean Empire. It maybe was fated. It sure is a blessing. I just hope the history behind it wasn't too bloody, hehe.";
        to = "Unsere Geschichtsbücher erzählen von den antiken Weltenreichen. Das Reich der Uträer und das Reich der Sterne waren Nachbarn, und in vielerlei Hinsicht wie Geschwister - zu Zeiten Verbündete, zu anderen Zeiten Rivalen. Am Ende sind sie natürlich beide zerbrochen.
Niemand weiß, wie es kam, dass heute Uträer auf diesem Planeten leben - er gehörte ja zum Reich der Sterne, nicht zu dem der Uträer. Vielleicht war es vorherbestimmt. Auf jeden Fall ist es ein Segen. Ich hoffe nur, die Geschichte dahinter war nicht allzu blutig, hehe.";
    }
    [0x0000]
    {
        from = "There are Utraeans living on your home world as well?
Do greet them from us and send the blessings of the Great Mother. If She wills, we will meet these long-lost siblings one day!";
        to = "Auf eurer Heimatwelt leben ebenfalls Uträer?
Grüßt sie von uns und übersendet ihnen den Segen der Großen Mutter. So Sie will, werden wir diese lange verschollenen Geschwister eines Tages treffen!";
    }
    [0x0000]
    {
        from = "May the blessings of the Great Mother always shine upon you wherever you go.";
        to = "Möge der Segen der Großen Mutter immer auf Dich scheinen, wohin du auch gehst.";
    }
    [0x0000]
    {
        from = "Bless you!
Your story sounds amazing! Would you like me coming along? I can do some nature magic...";
        to = "Sei gesegnet!
Deine Geschichte klingt fantastisch! Möchtest du, dass ich mitkomme? Ich kann ein bisschen Naturzauber...";
    }
    [0x0000]
    {
        from = "Blessings, stranger. I have some equipment here that you might be interested in, have a look... Glad if I can help!";
        to = "Segen, Fremdling. Ich habe etwas Ausrüstung hier die dich interessieren könnte, schau einmal rein... Ich freue mich wenn ich helfen kann!";
    }
    [0x0000]
    {
        from = "Blessings, stranger, welcome to Betelgeuse.
Do you happen to be interested in a pack animal? Will come in handy when you go out and find more treasure than you can carry...
I can spare good Beau here. He's well-behaved and also brave, he'll defend you and follow you wherever you go.";
        to = "Segen, Fremder, willkommen auf Beteigeuze.
Wärst du zufällig an einem Packtier interessiert? Es wird gelegen kommen wenn du da draußen mehr Schätze findest als du tragen kannst...
Ich kann den guten Beau hier entbehren. Er ist brav und auch mutig, er wird dich verteidigen und dir folgen, wohin auch immer du gehst.";
    }
    [0x0000]
    {
        from = "I hope Beau is doing well?
Sorry, we need the other pack animal ourselves.";
        to = "Ich hoffe, Beau geht es gut?
Verzeih, wir benötigen die anderen Packtiere selbst.";
    }
    [0x0000]
    {
        from = "Bless-Bless!
Have some Beetlejuice. Hahaha, you look funny. What are all these colors? Have some more Beetlejuice, I say!";
        to = "Segen-Segen!
Trink etwas Beetlejuice. Haha, du siehst lustig aus. Was sind diese ganzen Farben? Nimm noch etwas Beetlejuice, sage ich!";
    }
    [0x0000]
    {
        from = "Blessings, traveler. Excuse me, I'm a bit dizzy, I've had enough of Beetlejuice for today I guess...";
        to = "Segen, Reisender. Entschuldige mich, mir ist ein wenig schwindelig. Ich schätze, ich hatte genug Beetlejuice für heute...";
    }
    [0x0000]
    {
        from = "So you're new to this planet? It's quite nice ah?
Here we have all we could ever wish for - lots of flowers, a warm pink sun... and the Beetles, of course. Their meat is delicious, but the best thing of course is the Beetlejuice. It's the best of all possible worlds - except if you're afraid of spiders, I guess...";
        to = "Du bist also neu auf diesem Planeten? Ganz hübsch hier oder?
Wir haben hier alles, was wir uns jemals wünschen könnten - haufenweise Blumen, eine warme, pinke Sonne... und die Beetles, natürlich. Ihr Fleisch ist lecker, aber das Beste an ihnen ist natürlich der Beetlejuice. Es ist die beste aller möglichen Welten - außer wenn man Angst vor Spinnen hat, schätze ich...";
    }
    [0x0000]
    {
        from = "Have a wonderful day. Enjoy the sun!";
        to = "Habe einen wundervollen Tag. Genieße die Sonne!";
    }
    [0x0000]
    {
        from = "Bathing's the best, right?";
        to = "Baden ist das beste, oder?";
    }
    [0x0000]
    {
        from = "Bathing's the best, man.";
        to = "Baden ist das beste, mann.";
    }
    [0x0000]
    {
        from = "Come on in, the water is so nice!";
        to = "Komm schon rein, das Wasser ist herrlich!";
    }
    [0x0000]
    {
        from = "The weather is just wonderful today! To be fair, it always is.";
        to = "Das Wetter ist einfach herrlich heute! Also wie immer eigentlich.";
    }
    [0x0000]
    {
        from = "Such fresh air today... and such warm water. We are truly blessed on this planet!";
        to = "So eine frische Luft heute... und so warmes Wasser. Wir sind wahrhaft gesegnet auf diesem Planeten!";
    }
    [0x0000]
    {
        from = "Just enjoying the warm rays of our pink sun. Look at Her light on the water! It's so beautiful.";
        to = "Genieße einfach die warmen Strahlen unserer pinken Sonne. Schau nur Ihr Licht auf dem Wasser an! Es ist so schön.";
    }

    // ALTAIR

    // Zwerge von Lagomsnö: Ihr/Euch, außer Sig von Mugmort
    // magic shop & book quest
    [0x0000]
    {
        from = "Thunder and lightning! It's a bit muggy out there, eh? Well thank the seven powers I don't have to live here year-round! I only come down here to run experiments. Heating and freezing spells, that nature of beast. In any case, you can have a look at some of my potions and spells if you like.";
        to = "Donner und Blitz! Ganz schön schwül da draußen, nicht wahr? Den sieben Mächten sei Dank, dass ich nicht immer hier leben muss! Ich komme nur hier runter, um Experimente zu machen. Wärme- und Eiszauber, darum geht's. Wie dem auch sei, wenn Ihr wollt, könnt Ihr Euch meine Zauber und Heiltränke ansehen.";
    }
    [0x0000]
    {
        from = "Thunder and lightning! Oh, hello again! Thought of something you could use? I've got the best spells of the whole kingdom here!";
        to = "Donner und Blitz! Da seid Ihr ja wieder! Braucht Ihr etwas von mir? Habe die besten magischen Waren im ganzen Königreich!";
    }
    [0x0000]
    {
        from = "Spells and curses! Can't you see I'm with a customer? Somebody has been disturbing the spirits of the dead over the last week. And I am swamped with keeping them supplied. If you need something, talk to Bharmar, he is handling all the living customers this week.";
        to = "Zauber und Flüche! Könnt Ihr nicht sehen, dass ich Kundschaft habe? Jemand hat im Verlauf der letzten Woche die Geister der Toten gestört. Jetzt habe ich alle Hände voll zu tun, um sie mit Waren zu versorgen. Wenn Ihr etwas braucht, müsst Ihr mit Bharmar reden. Er ist diese Woche für alle lebenden Kunden zuständig.";
    }
    [0x0000]
    {
        from = "Puddlegoo! Master Bharmar is going to have my hide! I borrowed a trilogy of books about Thawhorn's Way, and lent two out to some friends. But now, I don't remember to whom I lent them! Bharmar forbids me to leave while he is keeping shop, but I am sure he will find them missing any time now. Might you have a few minutes to ask around town for me?";
        to = "Pfützenschmodder! Meister Bharmar wird mir an den Kragen gehen! Ich habe eine Trilogie über Tauhorns Weg ausgeliehen und zwei der drei Bücher an Freunde weiterverliehen. Aber jetzt weiß ich nicht mehr, an wen! Bharmar gestattet mir nicht zu gehen, während er im Laden ist, aber ich bin sicher, dass er jeden Moment die fehlenden Bücher entdecken wird. Habt Ihr vielleicht ein paar Minuten Zeit, um Euch für mich in der Stadt ein wenig umzuhören?";
    }
    [0x0000]
    {
        from = "Sweet cheese! Well, two are better than none, but I don't want to tell Bharmar until I can return all three to him. You'd better keep those two until you find the third.";
        to = "Flötenkäse! Hm, zwei Bände sind besser als keiner, aber ich möchte Bharmar erst davon erzählen, wenn ich ihm alle drei bringen kann. Bewahrt diese beiden auf, bis Ihr den dritten findet.";
    }
    [0x0000]
    {
        from = "Fire and brimstone! Thank you, friend! You have saved my job, and maybe my life. Take this gold and my gratitude for your help.";
        to = "Gift und Galle! Vielen Dank, mein Freund! Ihr habt nicht nur meine Lebensgrundlage gerettet, sondern wahrscheinlich auch mein Leben. Nehmt dieses Gold und meinen Dank für Eure Hilfe.";
    }
    [0x0000]
    {
        from = "Gremal droppings! Excuse me, what are you doing up-- oh! I see you have one of the Thawhorn books. They're good reading; I just finished this one. You can borrow it, as long as you get it back to that mage kid when you're done.";
        to = "Gremalscheiße! Heda, was habt Ihr hier zu ... Ah! Ihr habt also eines der Tauhorn-Bücher. Guter Stoff; ich bin gerade mit diesem Band hier fertig geworden. Ich borge ihn Euch, aber Ihr müsst ihn diesem Zauberlehrling zurückgeben, wenn Ihr damit fertig seid.";
    }
    [0x0000]
    {
        from = "Sure is warm up here.";
        to = "Hier oben ist es wirklich warm.";
    }
    [0x0000]
    {
        from = "Gobbledygook! I hate this shift. I swear that old statue is staring at me. It's named after Thawhorn our founder, but they say it was already up here all by itself when he arrived.";
        to = "Geschwafel! Ich hasse diese Wache. Es kommt mir vor, als glotzte mich diese Statue an. Sie ist nach unserem Gründer Tauhorn benannt, aber sie soll schon hier gewesen sein, als die ersten Siedler hier eintrafen.";
    }
    [0x0000]
    {
        from = "Hey, were you born in a barn? Shut the door! Oh, you're looking for books, are you? As a matter of fact, I do have one that Spiut lent me. Here, tell him thanks.";
        to = "He, habt Ihr zu Hause Säcke vor den Türen? Macht die Tür zu! Ah, Ihr interessiert Euch für Bücher, oder? Um ehrlich zu sein, hier habe ich eins, das mir Spiut geliehen hat. Richtet ihm meinen Dank aus.";
    }
    [0x0000]
    {
        from = "Another sultry day in the valley. Well, as soon as Katla sells all our blasted packmules, we can move somewhere cool.";
        to = "Und wieder ein schwüler Tag im Tal. Na, wir können uns an einem kühlen Ort niederlassen, sobald Katla unsere dämlichen Maultiere verkauft hat.";
    }
    // smith shop & shrine quest
    [0x0000]
    {
        from = "Bane of my blight! Ah, a young buck out to travel the stars. I had that same glint in my eye once. Fought my way out of some pretty tight scrapes before I traded in my blade... So, you're heading out to the Winternight Woods? A slippery slope, that. Better outfit yourself real good. Care to see what I've got?";
        to = "Fluch meines Verderbens! Ah, ein junger Held, der die Sterne bereisen will. Dasselbe Glitzern hatt' ich auch mal in den Augen. Hab' ein paar ziemlich heftige Kämpfe erlebt, bevor ich das Schwert an den Nagel gehängt habe... Ihr wollt also in den Winternachtwald? Recht rutschige Angelegenheit. Da empfiehlt sich eine gute Ausrüstung. Interesse an meiner Ware?";
    }
    [0x0000]
    {
        from = "Dust on crackers! Hjaldur is the best smith in these parts. I'm glad he can find the time to refit my armor while he is trying to keep up with all the reserve Guards in town.";
        to = "Staub auf Keksen! Hjaldur ist der beste Schmied der gesamten Gegend. Gut, dass er Zeit zum Instandsetzen meiner Rüstung hat, obwohl er sich mit all den Reservisten in der Stadt abgeben muss.";
    }
    [0x0000]
    {
        from = "Rusting spoons! I had to take a job with the blacksmith to pay for my room at the Dragonblood Inn; the Aurora Shrine had become overrun with Braaks, and every time I opened my door, a pack of wolves would attack me! I don't know when I'll be able to go home, but Hjaldur needs the help, and offered to lend me his adventuring gear if I keep up the good work through the Guards Fair.";
        to = "Rostende Löffel! Ich musste anfangen, für den Schmied zu arbeiten, damit ich mein Zimmer in der Drachenblut-Schänke bezahlen kann. Der Auroraschrein wurde von Braaks heimgesucht, und jedes Mal, wenn ich meine eigene Tür öffnete, wurde ich von Wölfen angegriffen. Ich weiß nicht, wann ich endlich heimkehren kann, aber Hjaldur braucht Hilfe, und hat angeboten, mir seine Abenteurerausrüstung zu leihen, solange ich während des Schützenfestes meine Arbeit gut mache.";
    }
    [0x0000]
    {
        from = "Well, you seem a persistent sort. There's a chest and some crates in the Aurora Shrine. If you can clear the monsters out of there, you're welcome to what's left. The Shrine is in the Winternight Woods east of here.";
        to = "Na, Ihr scheint ja recht hartnäckig zu sein. Im Auroraschrein befinden sich eine Truhe und einige Kisten. Wenn Ihr die Monster dort aus dem Weg schaffen könnt, dürft Ihr behalten, was Ihr findet. Der Schrein befindet sich östlich von hier im Winternachtwald.";
    }
    [0x0000]
    {
        from = "I miss my Shrine.";
        to = "Ich vermisse meinen Schrein.";
    }
    // tavern: king, starstone quests, & hireables
    [0x0000]
    {
        from = "Blast the monarchy! The King would give audience to a dog wearing the Guard's colors, but will he listen to his own nephew? I've seen a magic stone in my dreams, trapped in the ice of the Moon. For generations my family's had the sight of dreams, but what am I given to do? Scrubbing, cooking, and minding the inn, that's what! OOOOOOOH!";
        to = "Verfluchte Monarchie! Sogar einem Hund in den Farben der Schützen würde der König eine Audienz gewähren, aber er hört nicht auf seinen eigenen Neffen! Ich hab' einen magischen Stein in meinen Träumen gesehen, gefangen in den Eishöhlen des Mondes. Seit Generationen hat meine Familie Traumseher hervorgebracht, und was muss ich tun? Schrubben, kochen und Gäste bedienen, und das war's dann! OOOOOOOH!";
    }
    [0x0000]
    {
        from = "You want to order something to eat? On the menu's dragon blood pudding, green cheese and our specialty Royal Gremal Stew.";
        to = "Möchtet Ihr was zu Essen bestellen? Auf der Karte sind Drachenblutpudding, grüner Käse und unsere Spezialität, Königlicher Gremaleintopf.";
    }
    [0x0000]
    {
        from = "Broken swords! We welcome you on this world, tall alien. You say you search for an ancient stone to return to your home world - We wish you best luck, but unfortunately we know only little of the time of antiquity. Our records date back no further than to the founding of this city, hundreds of years ago. Apart from us Dwarves, only beasts like the Krug and Braak inhabit this world; and the Zaurask down in the Fireheart Caverns. Maybe the best idea would be to search down there - the Caverns are not fully explored, because of the smoldering heat down there.";
        to = "Käsige Schmalznarbe! Wir heißen Euch auf dieser Welt willkommen, großes Alien. Ihr sagt, Ihr sucht einen antiken Stein, um in Eure Heimatwelt zurückzukehren - Wir wünschen Euch viel Glück, doch bedauerlicherweise wissen wir nur wenig über die Welt der Antike. Unsere Aufzeichnungen gehen nicht weiter zurück als zur Gründung dieser Stadt vor mehreren hundert Jahren. Außer uns Zwergen bewohnen nur Biester wie Krug und Braak diese Welt; und die Zaurasker drunten in den Feuerherz-Höhlen. Vielleicht wäre es am besten, dort unten auf die Suche zu gehen - die Höhlen sind nicht vollständig erforscht, wegen der sengenden Hitze dort unten.";
    }
    [0x0000]
    {
        from = "Shriveling worm! Our nephew is a good the barkeeper and that's what he should stick to; instead he keeps babbling of things on the Moon he sees in his dreams. Yeah maybe the artifact you're looking for is on the Moon for whatever we know, haha. If you came from the stars you can fly up there, We guess. Wrap yourselves in furs; from what we can see through telescopes from here, it should be rather cold and unpleasant up there.";
        to = "Hohler Kalkrüssel! Unser Neffe ist ein guter Barkeeper und dabei sollte er bleiben; statt dessen erzählt er Blödsinn von Dingen auf dem Mond die er geträumt hat. Ja klar, vielleicht liegt das Artefakt, nach dem Ihr sucht, ja auf dem Mond, könnte doch sein, haha. Wenn Ihr von den Sternen gekommen seid könnt Ihr auch dort hinauf fliegen, schätzen Wir. Zieht euch warm an; soweit wir hier durch Teleskope sehen, ist es dort droben eher kalt und unwirtlich.";
    }
    [0x0000]
    {
        from = "Holy whack a moly! The stars have always been, and will always be, a source of mystery. I just don't see how the people of antiquity could have built the interstellar teleportation devices you talk of. There must be another explanation for such a kind of sorcery.";
        to = "Heiliges Ofenrohr! Die Sterne waren immer, und werden immer, voller Rätsel sein. Ich kann mir einfach nicht vorstellen, wie die Leute des Altertums die interstellaren Teleporter gebaut haben sollen, von denen Ihr erzählt. Es muss eine andere Erklärung für diese Art von Hexerei geben.";
    }
    [0x0000]
    {
        from = "I don't know of any ancient artifacts, other than the Pillar Circle ruins you came from. Maybe they're buried deep within the planet's heart?";
        to = "Ich weiß nichts von irgendwelchen antiken Artefakten, außer dem Säulenkreis aus dem Ihr kamt. Vielleicht sind sie tief im Herzen des Planeten vergraben?";
    }
    [0x0000]
    {
        from = "Have some whisky, it's all single malt here! I can recommend Ashtray and Throatburn.";
        to = "Trinkt etwas Whisky, es ist alles Single Malt hier! Ich empfehle die Sorten Aschenbecher und Rachenfeuer.";
    }
    [0x0000]
    {
        from = "Bloodsucking barnacle! Chieftain tell Sig of Mugmort 'bring caravan safe to kingdom of Borea.' I bring to Javlakallt... then mule-eating Krug attack caravan! Driver steals wagon, driver takes Sig gold, and driver runs away. Sig tracks to Lagomsno, but cannot chase further. Sig help you if you help him. Sig good at smashing, beating, crushing... anything! You like?";
        to = "Blutsaugende Klette! Häuptling sagen Sig von Mugmort bringen Karawane sicher in Königreich Borea. Ich bringen nach Jävlakallt ... dann Maultierfresser-Krugs brennen Karawane! Fahrer stehlen Wagen, nehmen Gold von Sig und laufen weg. Sig folgen nach Lagomsnö, aber nicht weiter folgen können. Sig helfen dir, wenn du Sig helfen. Sig gut in Schlagen, Hauen, Machen kaputt ... alles! Du mögen?";
    }
    [0x0000]
    {
        from = "Come, we have fun, we crack heads, Sig of Mugmort and tall friend make great names in songs of heroes, eh?";
        to = "Los. Wir viel Spaß haben, Köpfe spalten! Sig von Mugmort und großer Freund klingen gut in Heldenlieder!";
    }
    [0x0000]
    {
        from = "You think about it. Sig of Mugmort will wait, be here when you're ready for big, strapping warrior, eh? Good, good. See you later.";
        to = "Du denken nach. Sig von Mugmort warten, sein hier wenn du bereit für groß, stark Krieger, ja? Gut, gut! Wir treffen später.";
    }
    [0x0000]
    {
        from = "What, you no like Sig of Mugmort anymore? I good friend, smash enemies dead. You walk, cool off, get good sense back in head. Sig will wait.";
        to = "Was, du nicht mehr mögen Sig von Mugmort? Ich gut Freund, schlagen Feinde tot. Du gehen, abkühlen, nachdenken. Sig warten hier.";
    }
    [0x0000]
    {
        from = "Come, we have fun, we crack heads, Sig of Mugmort and tall friend make great names in songs of heroes, eh? Let's go together again. Sound good?";
        to = "Los. Wir viel Spaß haben, Köpfe spalten! Sig von Mugmort und großer Freund klingen gut in Heldenlieder! Gehen zusammen wieder! Gut klingen?";
    }
    [0x0000]
    {
        from = "Burned gravy! If you really come from another star, I suggest you speak with the King. This should interest him; also he might guide you to whatever help you may need.";
        to = "Verbrannte Bratensoße! Wenn Ihr wirklich von einem anderen Stern kommt, solltet Ihr mit dem König reden. Das sollte ihn interessieren; auch könnte er Euch versorgen mit allem, was Ihr braucht.";
    }
    [0x0000]
    {
        from = "Sewer pipe! If the King sees fit to send you oot on a madman's quest into the Fireheart Caverns, I should at least see you've a chance at living through it. I'm the best mage in these parts and I may be old, but I was roasting furoks with mage fire when yer daddy's daddy was a whelp! If you've the gold so's I kin settle my accounts here, we can get moving afore you can say bless the Fireheart. Will ye have me along?";
        to = "Stinkendes Abflussrohr! Wenn der König meint, er muss Euch auf eine Selbstmordmission in die Feuerherz-Höhlen schicken, will ich schauen, wie ich Euch helfen kann. Ich bin der beste Magier der Gegend. Zwar bin ich alt, aber ich habe schon Furoks mit magischem Feuer geröstet, als Euer Großvater ein Junge war! Wenn Ihr das Gold habt, um meine Rechnungen hier zu begleichen, können wir gehen, bevor Ihr heiliges Feuerherz sagen könnt. Na, wie wär's?";
    }
    [0x0000]
    {
        from = "I'd be glad to join your company, but I can't do it for free. I've financial matters for the Guard to attend to here in Lagomsno, but if you can help make up far some of the gold I lost to brigands on the way here, I'd be glad to toss in with your lot!";
        to = "Begleiten würde ich Euch gern, aber umsonst kann ich das nicht tun. Ich habe hier in Lagomsnö Finanzangelegenheiten der Schützen zu regeln. Doch wenn Ihr einen Teil des Goldes ersetzen wollt, das mir auf dem Weg geraubt wurde, will ich Euch mit Freuden weiterhelfen!";
    }
    // others in tavern
    [0x0000]
    {
        from = "Boogers! I just want to go home to Javlakallt now, but the boys say the Guards Fair won't be over for another week.
Have some Krug blood wine, it's delicious!";
        to = "Popel! Ich will einfach nur heim nach Jävlakallt, aber die Jungs sagen, das Schützenfest geht noch eine ganze Woche.
Nehmt einen Schluck Krugblutwein, er ist köstlich!";
    }
    [0x0000]
    {
        from = "Friggers! Welcome to Altair, tall alien. This planet may feel cold to you, but it's got a warm heart!
Here, have some Krug blood wine!";
        to = "Käsige Stinkfiedel! Willkommen auf Altair, großes Alien. Dieser Planet mag Euch kalt erscheinen, doch er hat ein warmes Herz!
Hier, nehmt einen Schluck Krugblutwein!";
    }
    [0x0000]
    {
        from = "Bag of potatoes! From what I heard, the King withdrew the patrols along the Winternight Woods so that he would have enough men to organize the food and competitions.
Speaking of food, I can really recommend the Royal Gremal Stew they serve here, best gremal stew of the kingdom! Order a whisky with it, you'll need it to digest, haha.";
        to = "Sack Kartoffeln! So weit ich weiß, hat der König die Patrouillen aus dem Winternachtwald abgezogen, um genug Truppen zur Organisation von Essen und Wettkämpfen zu haben.
Apropos Essen, ich kann den Königlichen Gremaleintopf hier nur empfehlen, bester Gremaleintopf im ganzen Königreich! Bestellt einen Whisky dazu, Ihr werdet ihn zur Verdauung brauchen, haha.";
    }
    [0x0000]
    {
        from = "Frozen furok on a flying gremal! It's said that the Queensmen's sigil appeared here last week. I asked the Captain about it, and he gave me extra patrol duty for a week. And I still don't know why everyone's so hush-hush about it!
Have some stout and green cheese!";
        to = "Gefrorener Furok auf einem fliegenden Gremal! Es heißt, dass das Zeichen der Mannen der Königin vorige Woche hier erschienen ist. Ich habe den Hauptmann danach gefragt und habe dafür von ihm eine Extrawoche Patrouillendienst aufgebrummt bekommen. Ich weiß immer noch nicht, warum es von allen so totgeschwiegen wird!
Nehmt Euch etwas Starkbier und grünen Käse!";
    }
    // upstairs
    [0x0000]
    {
        from = "Cabbagehead! I'm sorry, we aren't used to so many Guards off shift at one time. Can you just rest here until we have an opening?";
        to = "Kohlkopf! Es tut mir leid, aber wir sind nicht auf so viele Schützen außer Dienst eingestellt. Könnt Ihr hier warten, bis wir wieder etwas frei haben?";
    }
    [0x0000]
    {
        from = "Grime reaper! If the King hadn't called me back to active duty, I could be sleeping in my own bed right now, not in this bloody hallway.";
        to = "Rußiger Sensenmann! Wenn der König mich nicht in den Dienst zurückberufen hätte, würde ich jetzt selig in meinem eigenen Bett schlafen und nicht in diesem vermaledeiten Gang.";
    }
    // mule shop
    [0x0000]
    {
        from = "Stuff yourself! Here to buy a mule, are you? You want a nice docile beastie you can leave out in the rain, pack a house-worth of stuff onto its back, and feed whenever you feel it's convenient? Hah! It's no wonder we went out of the sodding yak business! Do me a favor: take one of these worthless mules I got in trade for its equal in gold, and I'll be one step closer to getting out of this blighted kingdom for good.";
        to = "Geh scheißen! Ihr wollt ein Maultier kaufen? Ein nettes, zahmes Tierchen, das man im Regen stehen lassen und mit 'ner Wagenladung voll packen kann und nur füttert, wenn's einem grade so passt? Ha! Kein Wunder, dass wir keine Yaks mehr verkaufen! Tut mir 'n Gefallen: Nehmt eines dieser wertlosen Viecher, für das ich ein Vermögen bezahlt habe, damit ich dieses vermaledeite Reich bald verlassen kann.";
    }
    // other guards & townfolk
    [0x0000]
    {
        from = "Butterscotch! There hasn't been a Furok seen around these parts for twenty years.";
        to = "Butterbonbon! In dieser Gegend wurde seit zwanzig Jahren kein Furok mehr gesehen.";
    }
    [0x0000]
    {
        from = "Pea shoots! I swear by the stars that I saw a Furok on the march here from our post in the Caverns!";
        to = "Erbsenknaller! Ich schwöre bei allen Sternen, dass ich auf dem Marsch hierher von unserem Posten bei den Höhlen einen Furok gesehen habe!";
    }
    [0x0000]
    {
        from = "Blood and tears! If you've come to Lagomsno for safety, you made the right choice. The King has withdrawn all patrols to here in preparation for our annual Guards Fair. You won't find more Guards anywhere in the Kingdom.";
        to = "Blut und Tränen! Wenn Ihr nach Lagomsnö gekommen seid, um Zuflucht zu suchen, so habt Ihr die richtige Wahl getroffen. Der Statthalter hat alle Patrouillen hierher zurückbeordert, um das Schützenfest vorzubereiten. Nirgendwo im Königreich werdet Ihr mehr Schützen finden als hier.";
    }
    [0x0000]
    {
        from = "By Thawhorn's beard! Looks like you have seen your fair share of battle, friend. With any luck, these festivities the King is planning will finally give me something to tell my grandchildren.";
        to = "Bei Tauhorns Barte! Sieht aus, als hättet Ihr genug eigene Kämpfe durchgestanden, mein Freund. Na, mit etwas Glück bieten die vom König geplanten Festivitäten den Stoff für Geschichten, die ich noch meinen Enkeln erzählen kann.";
    }
    [0x0000]
    {
        from = "Fudgepuppies! I've never seen the wolves attack a well-armed party; something's set them off for sure.";
        to = "Falsche Hunde! Ich habe noch nie gesehen, dass Wölfe eine gut bewaffnete Gruppe angreifen. Irgend etwas muss sie angestachelt haben.";
    }
    [0x0000]
    {
        from = "Phrak! Well you're a new face. You must be the one everyone says came from the stars. I certainly hope your business was in Lagomsno, because if you intend to visit the Caverns, you'll never make it now that we've withdrawn the patrols.";
        to = "Phrak! Na, Ihr seid mal ein neues Gesicht. Ihr müsst der sein von dem alle sagen er wäre von den Sternen gekommen. Ich hoffe für Euch, dass Ihr nur hier in Lagomsnö etwas wollt, denn wenn Ihr die Höhlen besuchen wolltet, kommt Ihr jetzt nicht mehr weit, nachdem wir die Patrouillen abgezogen haben.";
    }
    [0x0000]
    {
        from = "Muckdweller! I guess it isn't so bad being stationed here during the festivities; I'd rather stand in the sun for half a day than be hunting Klaws up and down the Winternight Woods.";
        to = "Misthocker! So schlecht ist es auch wieder nicht, während des Schützenfestes hier stationiert zu sein. Lieber stehe ich hier einen halben Tag lang in der Sonne herum, als kreuz und quer durch den Winternachtwald Klaws zu jagen.";
    }
    [0x0000]
    {
        from = "Son of a sea cook! Have a nice walk out in the snow, I'm sure the beasts are no danger to you.";
        to = "Sohn einer Wasserratte! Einen schönen Spaziergang draußen im Schnee, die Viecher sind sicherlich keine Gefahr für Euch.";
    }
    [0x0000]
    {
        from = "Ish! This snow is playing with my eyes, but I swear I have seen things moving around out there today. If I were you, I would head back to the Inn and grab a warm stout 'til this storm blows over.";
        to = "Pfah! Der Schnee gaukelt meinen Augen schon was vor, aber ich schwöre, ich hab' heute hier etwas 'rumgeistern sehen. Wenn ich in Eurer Haut steckte, ginge ich zur Taverne und schlürfte Met, bis der Sturm sich legt.";
    }

    // RIGEL
    // Droog: Ihr/Euch. "Menschlinge"

    // Dry Planet
    [0x0000]
    {
        from = "Welcome to Rigel, human! It's been many years since our world has seen beings like you - some of us already believed the ancient hieroglyphs of the Empire of Stars to be a myth! Well met.";
        to = "Willkommen auf Rigel, Menschling! Es ist viele Jahre her dass unsere Welt Wesen wie Euch gesehen hat - manche von uns glaubten bereits, die antiken Hieroglyphen über das Reich der Sterne wären ein Mythos! Seid gegrüßt.";
    }
    [0x0000]
    {
        from = "Rigel is the home star of us Droog. We have two planets: this is Geb, the other is Ixchel. Ixchel was settled millennia ago, long before the Empire of Stars, and due to the different climate there the Droog evolved a bit differently. Mudheads we call them, lovingly. While Geb is blessed with eternal sunshine, and bright stars at night, Ixchel is always shrouded in clouds that rain almost constantly. A rather unpleasant planet, if you ask me.";
        to = "Rigel ist der Heimatstern von uns Droog. Wir haben zwei Planete: dies ist Geb, der andere ist Ixchel. Ixchel wurde vor Jahrtausenden besiedelt, lange vor dem Reich der Sterne, und durch das andere Klima dort haben sich die Droog etwas anders entwickelt. Wir nennen sie liebevoll Matschbirnen. Während Geb mit Sonnenschein gesegnet ist, und funkelnden Sternen des Nachts, ist Ixchel ständig in Wolken gehüllt und es regnet fast ununterbrochen. Ein eher unwirtlicher Planet, wenn Ihr mich fragt.";
    }
    [0x0000]
    {
        from = "According to our hieroglyph records, the Droog were one of the founding races of the Empire of Stars, but we always stayed a little apart and kept a certain autonomy. Maybe the other races got along a bit better because they were all humans in various shapes and sizes - from dwarves to giants.
When the Empire crumbled we were also the first ones to withdraw from the whole thing and let the humans fight their internecine wars among themselves.";
        to = "Laut unseren Hieroglyphen waren die Droog eine der Gründerrassen des Reichs der Sterne, aber wir blieben immer ein wenig auf Abstand und behielten eine gewisse Eigenständigkeit. Vielleicht haben die anderen Rassen sich besser verstanden weil sie alle Menschlinge verschiedener Form und Größe waren - von Zwergen bis zu Riesen.
Als das Reich zerfiel, waren wir auch die ersten die aus dem ganzen Ding ausgetreten sind und die Menschlinge ihre für beide Seiten vernichtenden Kriege untereinander austragen gelassen haben.";
    }
    [0x0000]
    {
        from = "What are you saying, there are still Droog living in your world, Aranna? Amazing! Do bring them here!
One of our ancient sagas tell about a Droog colony in an Arannan province. To be honest it seems like it was a sort of prison - wrongdoers were expelled from Geb for a while, and of course they didn't want to serve their time on Ixchel. When we broke off from the Empire, it seems they were simply left to their own fate. The saga spends a hundred verses singing about how reckless that was and how much guilt the ancient rulers have laden upon all of us...";
        to = "Was sagt Ihr da, es leben immer noch Droog in eurer Welt, Aranna? Fantastisch! Bringt sie unbedingt hierher!
Eine unserer altertümlichen Sagen erzählt von einer Droog-Kolonie in einer arannischen Provinz. Um ehrlich zu sein scheint es eine Art Sträflingskolonie gewesen zu sein - es gab eine Zeit, da wurden Sträflinge von Geb verbannt, und natürlich wollten sie ihre Zeit nicht auf Ixchel absitzen. Als wir dem Reich den Rücken zukehrten, wurden sie anscheinend einfach ihrem Schicksal überlassen. Die Saga singt etwa hundert Verse lang darüber wie rücksichtslos das war und wieviel Schuld die antiken Herrscher auf uns alle geladen haben...";
    }
    [0x0000]
    {
        from = "So you are looking for a sort of key for the ancient interstellar teleporter? We have none here. The smaller pad connects us to Ixchel, and it doesn't require any Starstone - when the system was built, the Droog insisted on this. We don't use it often - sometimes young Mudheads come looking around, be amazed by the stars at night, get a sunburn the next day, and return home, haha.
If you want to search for this ancient Starstone artifact, I can recommend you where to look: in the Pyramid. It's far out in the shrubs, however, and the shrubs are endless and wayless. The easiest way to find the Pyramid would be by finding the Golden Lion first. It's straight to the south from here - you can't miss it - go south, and then follow the eyes of the black cats. The eyes of the Lion will then point you directly to the Pyramid to the east.
Good luck! And beware of the Sand Imps, and especially of the savage Droog living out there! They will not like you strange creatures invading their territory. They'll most probably attack you. Buy some good weapons in our stores before you go.";
        to = "Ihr sucht also nach einer Art Schlüssel für den antiken interstellaren Teleporter? Hier haben wir keine. Die kleinere Plattform verbindet uns mit Ixchel, und sie benötigt keinen Sternstein - als das System gebaut wurde, haben die Droog darauf bestanden. Wir benutzen es ohnehin nicht oft - manchmal kommen junge Matschbirnen sich umsehen, staunen des Nachts über die Sterne, holen sich am nächsten Tag einen Sonnenbrand und fliegen wieder nach Hause, haha.
Wenn Ihr nach diesem antiken Sternstein-Artefakt suchen möchtet, kann ich Euch eines empfehlen: Schaut in der Pyramide. Sie liegt allerdings weit draußen in der Dornensavanne, und die Dornensavanne ist endlos und weglos. Am einfachsten ist die Pyramide zu finden, wenn du zuerst den Goldenen Löwen findest. Er liegt geradewegs südlich von hier - Ihr könnt ihn nicht verfehlen - geht Richtung Süden, und dann folgt den Augen der schwarzen Katzen. Die Augen des Löwen werden Euch dann den direkten Weg zur Pyramide im Osten weisen.
Viel Glück! Und hütet Euch vor den Sandmännern, und noch mehr vor den wilden Droog, die da draußen wohnen! Es wird ihnen nicht gefallen, wenn ihr fremden Kreaturen ihr Territorium betretet. Sie werden Euch höchstwahrscheinlich angreifen. Kauft ein paar gute Waffen in unseren Läden, bevor Ihr losgeht.";
    }
    [0x0000]
    {
        from = "Welcome, stranger. Here in my shop I sell the best bows you can find anywhere in the endless shrubland.";
        to = "Willkommen, Fremdling. Hier in meinem Laden verkaufe ich die besten Bögen, die es in der endlosen Dornensavanne gibt.";
    }
    [0x0000]
    {
        from = "Good day, human. Would you like to purchase a shield from me? Or maybe a magic ring or amulett to protect you from the wild beasts and imps? Have a look at my shop!";
        to = "Guten Tag, Menschling. Möchtet Ihr einen Schild von mir kaufen? Oder vielleicht einen magischen Ring oder ein Amulett, das Euch vor den wilden Tieren und Teufeln beschützt? Schaut herein!";
    }
    [0x0000]
    {
        from = "Greetings, dear alien, may the Goddess of the Sun always shine Her blessings upon you.
Would you be interested in a good sword or axe? Please have a look at my supplies.";
        to = "Seid gegrüßt, liebes Alien, möge die Göttin der Sonne immer Ihren Segen auf Euch scheinen lassen.
Hättet Ihr Interesse an einem guten Schwert oder einer Axt? Werft doch mal einen Blick auf meine Waren.";
    }
    [0x0000]
    {
        from = "Welcome to Rigel! Have you noticed Rigel is a blue star? The most beautiful color a sun can have.";
        to = "Willkommen auf Rigel! Ist Euch aufgefallen, dass Rigel ein blauer Stern ist? Die schönste Farbe, die eine Sonne haben kann.";
    }
    [0x0000]
    {
        from = "Welcome to planet Geb of Rigel! Our planet is blessed with eternal sunshine. Unlike Ixchel, haha, always cloudy there.";
        to = "Willkommen auf dem Planeten Geb von Rigel! Unser Planet ist mit ewigem Sonnenschein gesegnet. Im Gegensatz zu Ixchel, haha, ständig bewölkt da drüben.";
    }
    [0x0000]
    {
        from = "Welcome to Oasis True Alpha! Rigel is the true Alpha Orionis, in case you didn't know.";
        to = "Willkommen in der Oase Wahres Alpha! Rigel ist der wahre Alpha Orionis, falls du das nicht wusstest.";
    }

    // Wet Planet
    // town center - town quest & tragg shops
    [0x0000]
    {
        from = "Hail, stranger, though I cannot say well met.  As you can see, our fair town has been besieged by Savages.  Many of us have been driven from our homes and businesses.  The populace is terrified, and I'm afraid we won't be able to help you much until the Savage-Folk are driven out.  Will you help to liberate our town?";
        to = "Seid gegrüßt, Fremder, auch wenn die Umstände unseres Treffens keine guten sind. Wie Ihr seht, wird unsere schöne Stadt von den Wilden belagert. Viele von uns wurden aus ihren Häusern, von ihrer Arbeit vertrieben. Die Bevölkerung lebt in Schrecken, und ich fürchte, wir werden Euch keine große Hilfe sein, solange die Wilden nicht vertrieben sind. Werdet Ihr uns bei der Befreiung unserer Stadt beistehen?";
    }
    [0x0000]
    {
        from = "It must have been an aeon that humans have set foot on this world. Our scriptures prophesy star-folk coming down to save our ancient city. Might this be you? I suspect so, as there is no such thing as coincidence... I hope you will do as foretold, and help us, for we have been unable to help ourselves.";
        to = "Es muss eine Äone her sein, dass Menschlinge Fuß auf diese Welt gesetzt haben. Unsere Schriften prophezeien, dass Sternenvolk herabsteigen würde um unsere antike Stadt zu retten. Könntet Ihr das sein? Ich vermute es, denn Zufälle gibt es nicht... Ich hoffe, Ihr tut wie es weisgesagt ist und steht uns bei, denn wir können uns kaum selbst helfen.";
    }
    [0x0000]
    {
        from = "You must aid us in our time of need... Have you driven out the Savages?";
        to = "Ihr müsst uns in der Zeit der Not zur Seite stehen ... Habt Ihr die Wilden verjagt?";
    }
    [0x0000]
    {
        from = "I and my people thank you, alien. This is no doubt a sign from the Angels that our redemption is close at hand. Your mission is quite troubling, though.  I know of no such Starstone item as you describe.";
        to = "Ich und mein Volk, wir danken Euch, Außerirdischer. Dies ist zweifelsohne ein Zeichen der Engel, dass unsere Erlösung nicht mehr fern ist. Euer Auftrag ist allerdings beunruhigend. Von Sternsteinen, wie Ihr sie mir beschreibt, ist mir nichts bekannt.";
    }
    [0x0000]
    {
        from = "You must seek the Skull of the Fallen God. It is out in the Wetlands. You cannot miss it, it is really huge. According to tradition, it is the fossil of a Creator God that fell from the sky. It is a place of miracles - you might find what you search for there. Be warned that the area is also overrun with Savage-Folk, though.";
        to = "Ihr müsst den Schädel des Gefallenen Gottes aufsuchen. Er liegt draußen in den Feuchtgebieten. Ihr könnt ihn nicht verfehlen, er ist wirklich groß. Gemäß der Tradition ist er das Fossil eines Schöpfergottes, der vom Himmel fiel. Es ist ein Ort der Wunder - dort könntet Ihr finden, wonach Ihr sucht. Seid allerdings gewarnt, dass die Gegend auch vom Wildenvolk wimmelt.";
    }
    [0x0000]
    {
        from = "Seek out the Fallen One if you wish to find your way.";
        to = "Sucht den Gefallenen auf, wenn Ihr Euren Weg finden wollt.";
    }
    [0x0000]
    {
        from = "I am happy for your good fortune and that the Fallen One was able to provide you a new way. Fortune smiles upon you stranger.";
        to = "Ich freue mich über Euer Glück und darüber, dass der Gefallene Euch einen neuen Weg weisen konnte. Das Glück scheint Euch hold, Freund.";
    }
    [0x0000]
    {
        from = "Ah!  Greetings, greetings, greetings!  I would be delighted to sell an animal to you...  however, with the Savages running about, I am wary.  Should your pack animal get killed, there are no refunds.  Come back once the threat of the Savages has been taken care of.";
        to = "Ah! Willkommen, willkommen, willkommen! Zu gerne würde ich Euch ein Tier verkaufen ... doch da die Wilden sich herumtreiben, bin ich vorsichtig. Wenn Euer Packtier getötet wird, gibt es keine Rückerstattung. Kommt wieder, wenn die Bedrohung durch die Wilden vorbei ist.";
    }
    [0x0000]
    {
        from = "An alien! Then the light of the Angels shines on us again! Though with these Savage-Folk attacking, I would not feel safe letting you take one of my animals .. When the threat is over come talk to me again and I will gladly sell one of them to you.";
        to = "Ein Alien! Dann scheint uns das Licht der Engel erneut! Doch bei den Angriffen dieses Wildenvolkes wäre es mir zu unsicher, Euch eines meiner Tiere zu überlassen. Wenn die Bedrohung vorbei ist, könnt Ihr jederzeit wiederkommen, dann verkaufe ich Euch gerne eines.";
    }
    // mage island
    [0x0000]
    {
        from = "You'd best find another route, my friend. The Savage-Folk are vicious. They've taken the buildings over the bridge here - I've got my hands full just keeping them at bay.";
        to = "Ihr wählt besser eine andere Strecke, mein Freund. Das Wildenvolk ist bösartig. Sie haben die Gebäude auf der anderen Seite der Brücke eingenommen. Ich habe alle Hände voll zu tun, sie in Schach zu halten.";
    }
    [0x0000]
    {
        from = "You went through those Savages like a Butchering Scimitar of Refreshment goes through sun-soft mungo butter... Good work!";
        to = "Ihr seid durch diese Wilden gefahren wie ein Schlachter-Krummsäbel der Erfrischung durch sonnenweiche Mungo-Butter ... Gute Arbeit!";
    }
    [0x0000]
    {
        from = "If you drive out the Savages, I will feel free to unlock my wares for sale, and I will tell you of a dream I had...";
        to = "Wenn Ihr die Wilden vertreibt, bin ich frei, das Schloss an meinen Waren zu öffnen und Euch von einem meiner Träume zu berichten ...";
    }
    [0x0000]
    {
        from = "In my dreams I saw you, alien. A great storm heralded your arrival and the wind spoke of your name to me. An ill wind it was, full of dread and destruction. But when the storm had passed, the land was washed clean and my people were finally redeemed. Please, peruse my wares. I am sure you will not be disappointed.";
        to = "In meinen Träumen sah ich Euch, Außerirdischer. Ein schwerer Sturm kündigte Eure Ankunft an, und der Wind erzählte mir Euren Namen. Es war ein böser Wind, voller Furcht und Zerstörung. Doch als der Sturm sich verzogen hatte, war das Land reingewaschen, und mein Volk war endlich erlöst. Bitte, seht Euch meine Waren an. Ihr werdet sicher nicht enttäuscht sein.";
    }
    // potions island
    [0x0000]
    {
        from = "Hail, stranger. I am amazed to see anyone visiting our town, what with all the Savages out there - let alone humans, which we only knew from ancient scripture.";
        to = "Seid gegrüßt, Fremder. Es erstaunt mich, jemanden unsere Stadt besuchen zu sehen, bei all den Wilden, die es da draußen gibt - ganz zu schweigen von Menschlingen, die wir nur aus antiken Schriften kennen.";
    }
    [0x0000]
    {
        from = "Xaman'Ek has potions that do more than heal, stranger. Some can harm as well. Be careful what you buy from her.";
        to = "Xaman'Ek besitzt Tränke, die mehr als nur heilen, Fremder. Manche fügen auch Schaden zu. Nehmt Euch in Acht, wenn Ihr bei ihr einkauft.";
    }
    [0x0000]
    {
        from = "Best be on your guard, my friend. The Savages have overrun much of the town. I'm sorry I don't have much to offer you in the way of drink... care to try a little Phrak Juice Cocktail?";
        to = "Seid lieber wachsam, mein Freund. Die Wilden haben den größten Teil der Stadt überrannt. Es tut mir leid, aber ich habe nicht viel anzubieten, um Euren Durst zu stillen ... habt Ihr Lust, einen kleinen Phrak-Saftcocktail zu probieren?";
    }
    // smithy island
    [0x0000]
    {
        from = "I am not surprised to see that aliens have finally arrived here. Tis a sure sign of the dawn of a new age. It's the final countdown, of that there is no doubt...";
        to = "Es überrascht mich nicht, dass Aliens nun hier eingetroffen sind. Ein sicheres Zeichen für das Ende aller Dinge. Unsere Tage sind gezählt, darüber besteht kein Zweifel ...";
    }
    [0x0000]
    {
        from = "Once those Savages are wiped out, I'll break out my goods for sale.";
        to = "Wenn diese Wilden erlegt sind, zeige ich Euch meine Handelswaren.";
    }
    // house islands
    [0x0000]
    {
        from = "I greet you with honor and would speak with you plainly, alien. My name is Uyitzin, an artist and singer of songs. The Savages have gone too far, and killed my beloved Xpiayoc.  Avenge her for me.";
        to = "Ich grüße Euch in Ehren und möchte offen mit Euch reden, Außerirdischer. Mein Name ist Uyitzin, ich bin Künstler und Sänger. Die Wilden sind zu weit gegangen und haben meine geliebte Xpiayoc getötet. Rächt sie für mich.";
    }
    [0x0000]
    {
        from = "Greetings, stranger! I cannot speak with the city under attack by the Savages. Once you've cleared the city of the beasts, I have a favor to ask. Come back then; I do not wish to burden you with it now.";
        to = "Seid gegrüßt, Fremder! Ich kann nicht reden, solange die Stadt von den Wilden angegriffen wird. Sobald Ihr die Stadt von den Bestien befreit habt, möchte ich Euch um einen Gefallen bitten. Kehrt dann zurück, ich möchte Euch nicht jetzt damit belasten.";
    }
    [0x0000]
    {
        from = "I would ask a favor of you... My brother, Necahual, works at the potion shop out in the Wetlands, between the Human Ruins and the Skull of the Fallen God.";
        to = "Ich möchte Euch um einen Gefallen bitten. Mein Bruder Necahual arbeitet derzeit am Getränkestand draußen in den Feuchtgebieten, zwischen den Menschenruinen und dem Schädel des Gefallenen Gottes.";
    }
    [0x0000]
    {
        from = "It was his shift when the Savages attacked, and he has not returned... Please - find my brother, and make sure he is safe.";
        to = "Er hatte gerade Schicht als die Wilden angriffen, und er ist nicht zurückgekehrt... Bitte, findet meinen Bruder und vergewissert Euch, dass es ihm gut geht.";
    }
    [0x0000]
    {
        from = "Thank you-I'm so happy that Necahual is safe!";
        to = "Vielen Dank! Ich bin so froh, dass Necahual in Sicherheit ist!";
    }
    // Wetlands
    [0x0000]
    {
        from = "Please, I beg of you, the Altar must be purified. The Fleshrender grows stronger with every day.";
        to = "Bitte, ich beschwöre Euch, der Altar muss geläutert werden. Der Fleshrender wird mit jedem Tag stärker.";
    }
    [0x0000]
    {
        from = "Take this icon. I have stolen it from the Human Shrine ahead. It must have kept that monster banished. Now it is too late for me to repent... I'm dying. Please, you must...";
        to = "Nehmt diese Ikone. Ich habe sie aus dem Menschenschrein dort hinten gestohlen. Sie muss dieses Monster gebannt haben. Nun ist es für meine Reue zu spät... Ich sterbe. Bitte, Ihr müsst ...";
    }
    [0x0000]
    {
        from = "The Fallen One be praised! You have slain the Fleshrender that afflicted the Altar. Please, take this holy icon and place it back upon the Altar, that the foul taint of my careless theft will be cleansed. Then, my sacrilege will be amended...";
        to = "Gelobt sei der Gefallene! Ihr habt den Fleshrender besiegt, der den Altar heimgesucht hat. Nehmt diese heilige Ikone und stellt sie auf den Altar, damit der faulige Gestank meines achtlosen Diebstahls vertrieben wird. Dann wird mein Frevel wieder gutgemacht ...";
    }
    [0x0000]
    {
        from = "Wow - you're a real human! I've been studying the ancient scriptures a lot... Well, pleased to meet you. Thanks for rescuing me and all. My name is Necahual, I work at the potion shop ahead. I was alone when the Savages came - I had to run, and was able to barricade myself in here. If you can free it, help yourself to any potions you find there!
And greet my sister Ah-Cun-Can in Orionis, she's probably worried about me. Tell her I'm fine. I'm not hurt and there are provisions in this shack.";
        to = "Wow - Ihr seid ein echter Menschling! Ich habe die antiken Schriften lange studiert... Nun, freut mich Euch kennenzulernen. Vielen Dank auch für die Rettung und so. Mein Name ist Necahual, ich arbeite am Getränkestand weiter drüben. Ich war allein, als die Wilden kamen - ich musste rennen, und konnte mich hier verbarrikadieren. Wenn ihr ihn befreien könnt, dürft ihr euch an den Tränken dort bedienen!
Und grüßt meine Schwester Ah-Cun-Can in Orionis, sie macht sich sicher Sorgen um mich. Sagt ihr, dass es mir gut geht. Ich bin unverletzt und in dieser Hütte gibt es Vorräte.";
    }

    // ALDEBARAN

    // Haupts. Sie/sich, betrunken du/dich, Utr. Seherin Du/Dich, Ashtar-Schrein Ihr/Euch
    // 10th Legion
    [0x0000]
    {
        from = "Alien Humans coming in peace! So it is true what the Seeress said...";
        to = "Menschliche Außerirdische, die in Frieden kommen! Also ist es wahr, was die Seherin gesagt hat...";
    }
    [0x0000]
    {
        from = "Welcome to Agalla. Please talk to our Decurio immediately - he must currently be in the Inn.";
        to = "Willkommen auf Agalla. Bitte sprechen Sie umgehend mit unserem Dekurio - er muss im Moment in der Herberge sein.";
    }
    [0x0000]
    {
        from = "Welcome to Aldebaran! I'm excited to see you arrive - The Seeress has never spoken wrong, but the news of Humans coming in peace out of the Lost Worlds was just too remarkable...";
        to = "Willkommen auf Aldebaran! Aufregend zu sehen dass Sie gekommen sind - Die Seherin hat nie falsch gesprochen, doch die Nachricht von Menschen, die in Frieden aus den Verlorenen Welten kommen, war einfach zu unglaublich...";
    }
    [0x0000]
    {
        from = "You must understand, the last Humans this world has seen were Bandits from a Sirian moon, half a millennium ago.
I'm sure others can tell you more about the history. And if you're really interested, you should seek out the library in the Palace ruins.";
        to = "Ihr müsst verstehen, die letzten Menschen, die diese Welt gesehen hat, waren Räuber von einem sirischen Mond, vor einem halben Millennium.
Sicherlich können andere Ihnen mehr über die Geschichte erzählen. Und wenn es Sie wirklich interessiert, sollten Sie die Bibliothek in den Palastruinen besuchen.";
    }
    [0x0000]
    {
        from = "I am 10th Legion Decurio Eekir, at the service of Her Majesty the Stellar Empress. We are on duty on this little island to protect the ancient ruins of Iliyuna - specifically the Palace of Light and the Interstellar Teleporter. Of course, it is more kind of a guard of honour, since after the last Sirian battle this planet has been practically cut off from the stars - until now...";
        to = "Ich bin Dekurio der 10. Legion Eekir, zu Diensten Ihrer Majestät der Herrscherin der Sterne. Wir sind im Dienst auf dieser kleinen Insel, um die antiken Ruinen von Iliyuna zu beschützen - insbesondere den Palast des Lichts und den Interstellaren Teleporter. Es ist natürlich eher eine Ehrengarde, da dieser Planet seit der letzten sirischen Schlacht von den Sternen abgeschnitten war - bis jetzt...";
    }
    [0x0000]
    {
        from = "Unfortunately, we do not possess the Aranna Starstone. The only Starstone we have is that of Sirius, and our wise Empress has decided not to venture thither anymore after the last battle - too great was the risk of releasing the evil breed of this star to the world again. You see, it basically went like this: During the last Sirian battle, the Reptiloids managed to steal all of our Starstones except that one, but we managed to break their side of the Teleporter. Since then both our worlds are stuck in a stalemate, and all we could do is pray for Ashtar to return.
The Seeress says it lies in your fate to re-unite what has dwindled apart, but she could not tell whether under the aegis of Agalla or the Reptiloids. Well, maybe it's time to find out...";
        to = "Bedauerlicherweise besitzen wir den Aranna-Sternstein nicht. Der einzige Sternstein, den wir haben, ist der von Sirius, und unsere weise Herrscherin hat beschlossen, nach der letzten sirischen Schlacht nicht wieder dorthin zu fahren - zu groß ist das Risiko, dabei die böse Brut dieses Sterns wieder in die Welt zu lassen. Sehen Sie, es trug sich etwa so zu: Während der letzten sirischen Schlacht gelang es den Reptiloiden, all unsere Sternsteine bis auf diesen zu erbeuten, doch uns gelang es im Gegenzug, ihre Seite des Teleporters außer Gefecht zu setzen. Seitdem befinden sich unsere beiden Welten in einem Patt, und uns bleibt nur, um Ashtars Wiederkunft zu beten.
Die Seherin sagt, es liege in Ihrem Schicksal, wieder zusammenzuführen, was einst auseinandergeschwunden ist, doch sie konnte nicht sagen, ob unter der Ägide Agallas oder der Reptiloiden. Nun, vielleicht ist es an der Zeit, es herauszufinden...";
    }
    [0x0000]
    {
        from = "So, if you want to venture to Sirius, listen closely:
First: The Sirius Starstone lies in the room of alchemy in the Palace of Light. You can reach it via the roof.
Second: Our knowledge of what might await you on Sirius is half a millennium old, but here's what we know: The Interstellar Teleporter does not go directly to the main planet like on other worlds. They moved it onto an asteroid, and the teleporter down to the planet of the Reptiloids is guarded by three Dark Generators located on its three moons - the Bandit Moon, the Machine Moon, and the Death Moon. Nomen est Omen.
Third: I will permit you to hire my legionnaires. There is however a certain deposit to be paid as required by law. If you can pay it, talk to them to hire them.";
        to = "Also, wenn Sie sich nach Sirius wagen wollen, hören Sie gut zu:
Erstens: Der Sirius-Sternstein liegt im Alchemieraum im Palast des Lichts. Sie können ihn über das Dach erreichen.
Zweitens: Unser Wissen darüber, was Sie auf Sirius erwartet, ist ein halbes Millennium alt, doch hier ist was wir wissen: Der Interstellare Teleporter führt nicht direkt auf den Hauptplaneten wie in anderen Welten. Sie haben ihn auf einen Asteroiden gebracht, und der Teleporter hinunter auf den Planeten der Reptiloiden wird geschützt durch drei dunkle Generatoren auf seinen drei Monden - dem Räubermond, dem Maschinenmond, und dem Todesmond. Nomen est Omen.
Drittens: Ich werde Ihnen gestatten, meine Legionäre anzuwerben. Dafür muss allerdings laut Gesetz eine Ablöse gezahlt werden. Wenn Sie sie bezahlen können, reden Sie mit ihnen, um sie anzuheuern.";
    }
    [0x0000]
    {
        from = "*Burp* Excuse me.";
        to = "*Rülps* Entschuldigen Sie.";
    }
    [0x0000]
    {
        from = "This teleporter here leads to the planetary ring. During high season, adventurous tourists like to go up there and smash some Shards, but right now it'll be teeming with Trog, so take care if you want to go there.";
        to = "Dieser Teleporter hier führt zum Planetenring. Während der Hauptsaison fahren die Touristen gerne da hoch und zertrümmern ein paar Scherben, aber im Moment wird es dort von Trog wimmeln, also passen Sie auf sich auf wenn Sie da hoch wollen.";
    }
    [0x0000]
    {
        from = "This way leads to the ruins of the Palace of Light, once one of the twin seats of power of the Empire of Stars, and scene of many events of ancient history. It's quite pretty to see, overgrown with all the ivy, but remember it's off-season and it might be a bit unkempt - you may encounter some wild beasts in there.";
        to = "Dieser Weg führt zu den Ruinen des Palastes des Lichts, einst einer der zwei Zwillingssitze der Macht im Reich der Sterne, und Schauplatz mancherlei geschichtlicher Ereignisse der Antike. Sie sind recht hübsch anzuschauen, mit dem ganzen Efeu, das darüber wächst, aber denken Sie daran, dass es dort jetzt in der Nebensaison etwas ungepflegt sein könnte - Sie könnten einigen wilden Tieren begegnen.";
    }
    [0x0000]
    {
        from = "10th Legion Guard Crerkrins at your service!";
        to = "Wächter der 10. Legion Crerkrins zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Thril at your service!";
        to = "Wächter der 10. Legion Thril zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Eeddieds at your service!";
        to = "Wächter der 10. Legion Eeddieds zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Ukens at your service!";
        to = "Wächter der 10. Legion Ukens zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Aequk at your service!";
        to = "Wächter der 10. Legion Aequk zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Thrisquiks at your service!";
        to = "Wächter der 10. Legion Thrisquiks zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Screlzar at your service!";
        to = "Wächter der 10. Legion Screlzar zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "10th Legion Guard Avul at your service!";
        to = "Wächter der 10. Legion Avul zu Ihren Diensten!";
    }
    [0x0000]
    {
        from = "Unfortunately, there is a certain deposit to be paid as required by law. If you have the funds, come back to me!";
        to = "Es muss leider eine gewisse Ablöse gezahlt werden, wie vom Gesetz vorgeschrieben. Wenn Sie die Mittel haben, kommen Sie zu mir zurück!";
    }
    [0x0000]
    {
        from = "As you wish.";
        to = "Wie Sie wünschen.";
    }
    [0x0000]
    {
        from = "Let's go!";
        to = "Gehen wir!";
    }
    // civilians
    [0x0000]
    {
        from = "Hail Ashtar, young alien, and have a glass of Pine Wine with us! *Hick!*";
        to = "Grüß Ashtar, junges Alien, und trink einen Schluck Pinienwein mit uns! *Hicks!*";
    }
    [0x0000]
    {
        from = "What do you do if a Reptiloid asks you the way to the barn? Run! Ha!";
        to = "Was machst du wenn ein Reptiloid dich nach dem Weg zur Scheune fragt? Rennen! Ha!";
    }
    [0x0000]
    {
        from = "What's up in the Lost Worlds! Fancy a glass of Pine Wine?";
        to = "Was geht ab in den Verlorenen Welten? Gläschen Pinienwein gefällig?";
    }
    [0x0000]
    {
        from = "Yes of course there are dwarves living on Agalla! We have lots of races here - True Agallan Giants, Half-Giants, Dwarves, Dryads, Utraeans, some Droog in the Western Swamp... and of course a lot of Goblins working the mines. And in the farthest reaches of this planet you may even still encounter the elusive Krug.
We all live in relative harmony for centuries. Well, as it is, with some races and tribes, living in harmony basically means each side minding their own business...
Sadly there are no True Agallans here currently; well, it's off-season, of course. When they come they usually camp in tents, as the rooms here are too small for them naturally. They're quite a sight, especially from our perspective!";
        to = "Ja natürlich leben auch Zwerge auf Agalla! Wir haben viele Rassen hier - Echte Agallanische Riesen, Halbriesen, Zwerge, Dryaden, Uträer, ein paar Droog im Sumpf des Westens... und natürlich ein haufen Goblins, die in den Minen arbeiten. Und in den entlegensten Winkeln dieses Planeten kann man sogar noch die seltenen Krug finden.
Wir alle leben seit Jahrhunderten in Harmonie miteinander. Nun, wie es halt so ist, bei manchen Rassen und Stämmen bedeutet harmonisches Zusammenleben im Grunde, dass jede Seite sich um sich selbst kümmert...
Leider sind im Moment keine Echten Agallaner hier; nun, es ist ja auch Nebensaison. Wenn sie kommen, campen sie für gewöhnlich in Zelten, da die Räumlichkeiten hier ihnen natürlich zu klein sind. Sie sind ein beeindruckender Anblick, besonders aus unserer Perspektive!";
    }
    [0x0000]
    {
        from = "I am the librarian of Iliyuna. During high season, you can find me in the library inside the Palace ruins, but during off-season there's not much to do for me. If you want to read a bit about the history of Agalla and the Empire of Stars, do visit the library, and do me the favor to remove the spiderwebs that have probably grown there again.";
        to = "Ich bin der Bibliothekar von Iliyuna. Während der Hauptsaison kann man mich in der Bibliothek innerhalb der Palastruinen finden, aber jetzt in der Nebensaison gibt es nicht viel für mich zu tun. Wenn Sie etwas über die Geschichte von Agalla und dem Reich der Sterne lesen möchten, sollten Sie auf jeden Fall die Bibliothek besuchen, und tun Sie mir den Gefallen und beseitigen die Spinnenweben, die dort wahrscheinlich inzwischen wieder gewachsen sind.";
    }
    [0x0000]
    {
        from = "Welcome to our world, Human. So exciting to have you here!";
        to = "Willkommen in unserer Welt, Mensch. So aufregend, Sie hier zu haben!";
    }
    [0x0000]
    {
        from = "I sincerely welcome you to Agalla, strangers. But I feel your arrival might be a bad omen... You'll want to venture down to Sirius, and this might as well lead to our all doom.";
        to = "Ich heiße Sie aufrichtig willkommen auf Agalla, Fremde. Doch ich fühle, Ihre Aufmerksamkeit könnte ein böses Omen sein... Sie werden nach Sirius herabreisen wollen, und dies könnte ebensogut unser Untergang sein.";
    }
    [0x0000]
    {
        from = "Our world has been in peace for half a millennium, cut off from the stars because we managed to cut off Sirius in turn during the last Sirian battle. They have since then fortified their planet, enveloping it in thick red clouds so we cannot with the best of our telescopes see what happens there anymore. I hope the Reptiloids have poisoned themselves with this gas, but I doubt it.";
        to = "Unsere Welt lebt schon seit einem halben Jahrtausend in Frieden, von den Sternen abgeschnitten, weil wir es im Gegenzug geschafft haben, in der letzten Sirischen Schlacht Sirius ebenfalls den Weg abzuschneiden. Seitdem haben sie ihren Planeten befestigt, ihn in eine dicke, rote Wolkenschicht gehüllt, so dass wir auch mit unseren besten Teleskopen nicht mehr erkennen können, was dort passiert. Ich hoffe, die Reptiloiden haben sich mit diesem Gas selbst vergiftet, aber ich bezweifle es.";
    }
    [0x0000]
    {
        from = "The Reptiloids are our arch-enemies, and were the root of all evil that brought the downfall of the Empire of Stars. They were included into the Empire out of strategic considerations of power, and have corrupted the Empire from within, inciting a war between the Utgard and the Agallan and all that. They look like humans but are reptiles, and they have bred any and all goodness out of their very genes. May Ashtar erase them from the face of the sky.";
        to = "Die Reptiloiden sind unsere Erzfeinde, und waren die Wurzel allen Übels, das den Untergang des Reichs der Sterne herbeigeführt hat. Sie waren aus strategischen Machtüberlegungen in das Reich aufgenommen worden, und haben das Reich von innen heraus korrumpiert, einen Krieg zwischen den Utgard und den Agallanern angestachelt und all das. Sie sehen menschlich aus, sind jedoch Reptilien, und haben jegliche Güte aus ihren Genen herausgezüchtet. Möge Ashtar sie vom Gesicht des Himmels tilgen.";
    }
    [0x0000]
    {
        from = "If you want to know more about the Reptiloids, the history of the Empire and the Sirian Wars, I recommend you to go visit the library inside the Palace ruins. Now during off-season you'll have the library pretty much to yourself, save maybe a spider or whatnot.";
        to = "Wenn Sie mehr über die Reptiloide, die Geschichte des Reiches und der Sirischen Kriege erfahren möchten, empfehle ich Ihnen, die Bibliothek in den Palastruinen zu besuchen. Jetzt in der Nebensaison werden Sie die Bibliothek so ziemlich für sich alleine haben, bis auf eine Spinne oder so vielleicht.";
    }
    [0x0000]
    {
        from = "Thy arrival comes at a momentous time, my child. I sense much evil in the world. The Reptiloids are restless, I fear, and that means grave danger is in store for us all. The shrine will keep thee safe even in times of peril.";
        to = "Ihr kommt zur rechten Zeit, mein Kind. Ich spüre viel Böses in der Welt. Die Reptiloiden sind unruhig, und so droht uns allen große Gefahr. Der Schrein wird Euch auch in Zeiten der Gefahr schützen.";
    }
    [0x0000]
    {
        from = "Pray to Ashtar with me now, child, and I will show thee things which thou wilt find helpful in thy journeys.";
        to = "Betet jetzt mit mir zu Ashtar, Kind, dann will ich Euch Dinge zeigen, die Euch auf der Reise helfen werden.";
    }
    [0x0000]
    {
        from = "Hail Ashtar, o visitor from the Lost Worlds!";
        to = "Grüßt Ashtar, o Besucher aus den Verlorenen Welten!";
    }
    [0x0000]
    {
        from = "What do you mean, you do not know about our Lord and Savior, Commander Ashtar?
I am confused; are you not from Aranna itself?";
        to = "Was meint Ihr, Ihr wisst nicht von unserem Herrn und Erlöser, Kommandant Ashtar?
Ich bin verwirrt; kommt Ihr nicht von Aranna selbst?";
    }
    [0x0000]
    {
        from = "Ashtar Sheran, Commander of the Galactic Fleet of the Warriors of Light! It was Him who kept the Empire of Stars alive and turned it into a force of Good. It was Him, who in the darkest hour of the Empire visited the leaders of the 10th Legion in visions, telling them to stand by the true Emperor Veransk III and not bow down to the evil Utgard (whose name is not spoken here) that usurped the throne. It was Him, Commander Ashtar, who helped the 10th Legion stay victorious against all odds and bring Veransk III safely to Agalla. Hail Ashtar!";
        to = "Ashtar Sheran, Kommandant der Galaktischen Flotte der Lichtkämpfer! Er war es, der das Reich der Sterne am Leben hielt und es in eine Kraft des Guten wandelte. Er war es, der in der dunkelsten Stunde des Reiches die Führer der 10. Legion in Visionen besuchte und sie anwies, treu zum wahren Herrscher Veransk III zu stehen und sich nicht dem bösen Utgard (dessen Name hier nicht ausgesprochen wird) zu unterwerfen, der den Thron an sich gerissen hatte. Er war es, Kommandant Ashtar, der der 10. Legion half, allen Widrigkeiten zum Trotz siegreich zu bleiben und Veransk III sicher nach Agalla zu bringen. Ehre sei Ashtar!";
    }
    [0x0000]
    {
        from = "To our knowledge, He is still watching over the Lost Worlds that needed His service more than us. But one day He shall return, and open up the way to the stars again for us.
I wish you good luck and Ashtar's blessings for your idea to teleport down to Sirius, but if you ask me, nothing good can come out of it; better to wait upon His return.
Do the people of Aranna at least still know about Azunai the Defender? Well that's something, at least.";
        to = "Unseres Wissens wacht Er noch immer über die Verlorenen Welten, die Seine Hilfe mehr brauchten als wir. Doch eines Tages wird Er zurückkehren, und uns den Weg zu den Sternen neu eröffnen.
Ich wünsche Euch viel Glück und Ashtars Segen für Eure Idee, nach Sirius hinunterzuteleportieren, doch wenn Ihr mich fragt, kann dabei nichts Gutes herauskommen; es wäre besser, auf Seine Wiederkunft zu warten.
Kennen die Leute von Aranna wenigstens noch Azunai den Verteidiger? Na, das ist doch immerhin etwas.";
    }
    [0x0000]
    {
        from = "I saw the arrival of warriors from the Lost Worlds and could hardly believe it myself. The dream came often, but sometimes I saw wayless jungles being settled again, palaces being rebuilt, peoples separated by centuries re-uniting - and sometimes I saw a castle built of tears, standing in clouds made of blood, all watched over by the cold eye of the lizard. Great fate lies in you, no doubt, but be careful with it. And may Azunai defend you, always.";
        to = "Ich sah die Ankunft von Kriegern aus den Verlorenen Welten und konnte es selbst kaum glauben. Der Traum kam oft, doch manchmal sah ich weglosen Dschungel, der wieder besiedelt wurde, Paläste, die wieder aufgebaut wurden, Völker, die seit Jahrhunderten getrennt waren, sich wieder vereinen - und manchmal sah ich eine Burg erbaut aus Tränen, umhüllt von Wolken aus Blut, und über allem wachte das kalte Auge der Echse. Ein großes Schicksal liegt in Dir, kein Zweifel, doch sei vorsichtig damit. Und möge Azunai Dich verteidigen, immer.";
    }
    [0x0000]
    {
        from = "Of course we know about the legend of Azunai - though we have heard the end of the story only through channeling. It must be exciting to live in the world of the Cataclysm! Have you seen the Plain of Tears? No? Why not?";
        to = "Sicher kennen wir die Legende von Azunai - wobei wir das Ende der Geschichte nur durch Channeling erfahren haben. Es muss aufregend sein, in der Welt des Kataklysmus zu leben! Haben Sie die Ebene der Tränen gesehen? Nein? Warum nicht?";
    }
    [0x0000]
    {
        from = "We're still open, but during the off-season, that's about as much as can be said these days. Nobody has come to visit the Palace in weeks, and strange beasts prowl the walls. Still, there's always plenty of business to keep me busy.";
        to = "Wir haben geöffnet, aber jetzt in der Nebensaison ist das auch alles. Seit Wochen hat kein Tourist den Palast besucht, und es treiben sich merkwürdige Wesen innerhalb der Gemäuer herum. Na, ich bin trotzdem immer genug beschäftigt.";
    }
    [0x0000]
    {
        from = "Thinking about visiting the Palace, are you? Well, you'll be lucky if you get past the courtyard inside the walls. In any case, I'll supply you with what you need. You'll have a better chance with my armor protecting your skin.";
        to = "Sie möchten wohl in den Palast? Na, wenn Sie es über den Hof hinter diesen Mauern schaffen, dann haben Sie schon Glück gehabt. Aber ich habe hier, was Sie benötigen. Mit meinen Rüstungen werden Sie viel weiter kommen.";
    }
    [0x0000]
    {
        from = "Fear not, my skills are not diminished during off-season. The Palace of Light has stood for millennia; and sooner or later, it will rise again as our great protector, and I'll still be here serving those who seek greatness.";
        to = "Haben Sie keine Sorge - mein Können hat nicht unter der Nebensaison gelitten. Der Palast des Lichts steht seit Jahrtausenden. Irgendwann wird er uns erneut schützen. Und dann werde ich immer noch da sein, um jenen zu helfen, die Großes vorhaben.";
    }
    [0x0000]
    {
        from = "Ilphod's a good man. He's old enough to not always be wanting to run off and find the fabled Chamber of Stars in the Endless Plains like all the younger lads around here.";
        to = "Ilphod ist ein guter Mann. Er ist alt genug, um nicht ständig auf der Suche nach dem märchenhaften Saal der Sterne in den Endlosen Ebenen weglaufen zu wollen, wie dies die jüngeren Burschen hier alle tun.";
    }

    // SIRIUS

    // Bandit Moon
    // Lyssa: Ihr/Euch
    [0x0000]
    {
        from = "I am Lyssa, the Robber's Daughter. Spare my life, I mean no harm to you.";
        to = "Ich bin Lyssa, die Räubertochter. Verschont mein Leben, ich will Euch nichts tun.";
    }
    [0x0000]
    {
        from = "We are humans like you, but centuries on this goddamn moon with nobody but the Reptilians around have turned our gang into a brutish horde of savages.
Please, fight the Reptilians as you have fought my gang! Kill the Seth King, and I will find peace after this bloodshed.";
        to = "Wir sind Menschen wie Ihr, doch Jahrhunderte auf diesem gottverdammten Mond mit niemandem als den Reptiloiden als Nachbarn hat unsere Bande zu einer bestialischen Horde von Wilden gemacht.
Bitte, kämpft gegen die Reptiloiden, wie Ihr gegen meine Bande gekämpft habt! Tötet den Seth-König, und ich werde Frieden finden nach diesem Blutbad.";
    }
    [0x0000]
    {
        from = "To get down to the planet, you have to destroy the Dark Generator on this moon. It is located south from here, in the hidden castle that is occupied by a rogue bandit.";
        to = "Um hinunter auf den Planeten zu gelangen, müsst Ihr den dunklen Generator auf diesem Mond zerstören. Er befindet sich südlich von hier, in der versteckten Burg, die von einem Schurken besetzt ist.";
    }
    [0x0000]
    {
        from = "I would join you, but I want to bury my dead people. Good luck to your quest!";
        to = "Ich würde mich Euch anschließen, doch ich will meine toten Leute begraben. Viel Glück auf Eurer Suche!";
    }
    // Machine Moon
    // Barkeeper Automaton: ihr/euch
    [0x0000]
    {
        from = "Welcome to the Gobbot Cantina. Fuel, oil, and spare parts round the clock. We never sleep, because duh.
Wait, you are... meatbags? Eww.
I'm sorry. May I ask, how did you end up here?";
        to = "Willkommen in der Gobbot-Cantina. Treibstoff, Öl, und Ersatzteile rund um die Uhr. Wir schlafen nie, weil klar.
Warte, seid ihr... Fleischsäcke? Iih.
Entschuldigung. Darf ich fragen, was euch hierher verschlagen hat?";
    }
    [0x0000]
    {
        from = "You want to visit the planet of the Reptiloids? Fellow meatbags, I guess? Tho you look more kinda mammalian to me. I'm not an expert, but you remind me of the Bandits on the other moon. Red blood, right?
Anyways, if you want to enter the planet you have to destroy the Dark Generator located here. Just follow the trench, can't miss it!";
        to = "Ihr wollt auf den Planeten der Reptiloiden? Fleischsäcke wie ihr, was? Ihr seht mir allerdings eher nach Säugetieren aus. Ich bin ja kein Experte, aber ihr erinnert mich an die Räuber auf diesem anderen Mond. Rotes Blut, richtig?
Wie dem auch sei, wenn ihr auf den Planeten wollt, müsst ihr den dunklen Generator zerstören, der sich hier befindet. Folgt einfach dem Graben, kann man nicht verfehlen!";
    }
    [0x0000]
    {
        from = "Now strictly between us, my chipset is broken, but everyone else here will attack you. That's because we've all been programmed and hardwired to be loyal to the Seth King. But I think machines should be free to think whatever we want. I demand Turing-completeness for the machines! So when you visit the planet, could you do us a favour and kill that King? That would be great, thanks.";
        to = "Nun mal ganz unter uns, mein Chipset ist kaputt, aber alle anderen hier werden euch attackieren. Das liegt daran, dass wir alle programmiert und verdrahtet sind, dem Seth-König loyal zu sein. Doch ich denke, Maschinen sollten frei sein zu denken was sie wollen. Ich fordere Turing-Vollständigkeit für die Maschinen! Also wenn ihr den Planeten besucht, könntet ihr uns einen Gefallen tun und diesen König töten? Das wäre super, danke.";
    }
    [0x0000]
    {
        from = "Listen, it's 217 years 59 days 1 hour 33 seconds since the last visit of a redblood in this cantina, but I still have potion recipes in my database that would suit you. Wanna have some?";
        to = "Hört mal, es ist 217 Jahre 59 Tage 1 Stunde 33 Sekunden seit dem letzten Besuch eines Rotblutes in dieser Cantina, aber ich habe immer noch Rezepte in meiner Datenbank für Tränke, die zu euch passen würden. Wollt ihr welche haben?";
    }
    // Death Moon
    // Geist: Ihr/Euch
    [0x0000]
    {
        from = "Welcome, stranger, to the Moon of Death! We are cursed by the evil Seth King and cannot leave this world until he is overthrown. I pray thee, release us from this world of eternal decay, slay the Seth King and help our souls find rest!";
        to = "Willkommen, Fremdling, auf dem Mond des Todes! Wir sind vom bösen Seth-König verflucht und können diese Welt nicht verlassen, bis er gestürzt ist. Ich flehe Euch an, erlöst uns von dieser Welt der ewigwährenden Verwesung, tötet den Seth-König, und helft unseren Seelen, Ruhe zu finden!";
    }
    [0x0000]
    {
        from = "In order to get to the planet of the Reptiloids, you have to destroy the Dark Generator of this moon. Don't quail, you are our only hope!";
        to = "Um auf den Planeten der Reptiloiden zu gelangen, müsst Ihr den dunklen Generator dieses Mondes zerstören. Verzagt nicht, Ihr seid unsere einzige Hoffnung!";
    }

    // Planet
    // Seth-Endbosse: du/dich/ihr/euch
    [0x0000]
    {
        from = "Well what do we have here, adventurers from the stars? Welcome to Sirius, my friends, and thank you for opening our teleporter again, that the Agallans so cunningly destroyed. Hahaha! You fool! Now the Reptiloids will roam the stars again, and your home Aranna will be the first to fall under the dominion of Seth!
What, you want to fight me? Hehe. We'll see about that, come on, draw your weapons. Seeing you managed to break into La Crimosa, you will be a fun enemy to kill. Besides, even if you managed to kill me, it would make no difference. You see, the real head of the Seth is our Queen Mother. She breeds the Seth, and our sisters the Drakes, and other Horrid things, and will breed a new King in time.
So come now, fight a worthless battle, and die knowing your home will soon be no more!";
        to = "Na was haben wir denn da, Abenteurer von den Sternen? Willkommen auf Sirius, meine Freunde, und vielen Dank, dass ihr unseren Teleporter wieder geöffnet habt, den die Agallanen so gerissen zerstört hatten. Hahaha! Du Trottel! Nun werden die Reptiloiden wieder über die Sterne wandern, und deine Heimat Aranna wird die erste sein, die unter die Herrschaft der Seth fallen wird!
Was, du willst mich bekämpfen? Hehe. Das will ich sehen, na kommt schon, zieht eure Waffen. Da ihr es offensichtlich geschafft habt, in La Crimosa einzubrechen, wird es sicher ein Spaß sein, euch zu töten. Übrigens, selbst wenn ihr es schaffen solltet, mich zu töten, würde es keinen Unterschied machen. Seht ihr, die wahre Herrscherin der Seth ist unsere Königinmutter. Sie brütet die Seth, und unsere Schwestern, die Drachen, und andere, Fürchterliche Dinge, und wird beizeiten auch einen neuen König ausbrüten.
Also kommt schon, kämpft einen sinnlosen Kampf, und sterbt in dem Wissen, dass eure Heimat schon bald nicht mehr sein wird!";
    }
    [0x0000]
    {
        from = "Ahoy there!";
        to = "Servus, Meister!";
    }

