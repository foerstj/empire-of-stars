        from = "Find an Aranna Starstone on Sirius to get back home.";
        to = "Finde einen Aranna-Sternstein auf Sirius um nach Hause zurückzukehren.";
    }
    [0x0000]
    {
        from = "After an odyssey among the stars, you are finally able to return home to Aranna.";
        to = "Nach einer Odyssee zwischen den Sternen kannst du nun endlich nach Aranna heimkehren.";
    }
