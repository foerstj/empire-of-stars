        to = "Finde Tante Luna";
    }
    [0x0000]
    {
        from = "After having received a letter informing you that your strange but beloved Aunt Luna has disappeared, you took the long journey to the place she was last seen. You hope to find her alive and well.";
        to = "Nachdem deine seltsame aber geliebte Tante Luna verschwunden ist, bist du den langen Weg dorthin gereist wo sie zuletzt gesehen wurde. Du hoffst, sie wohlauf zu finden.";
    }
    [0x0000]
    {
        from = "You found Aunt Luna alive and well.";
        to = "Du hast Tante Luna wohlauf gefunden.";
    }
    [0x0000]
    {
        from = "Find Way Home";
        to = "Finde einen Weg nach Hause";
    }
    [0x0000]
    {
        from = "Oh no, you went into the Interstellar Hub without the right starstone to get back home! You need to find an Aranna Starstone. Maybe Aunt Luna can help you... But she's on Betelgeuse.";
        to = "Oh nein, du bist in den Interstellaren Hub geflogen ohne den richtigen Sternstein der dich zurückbringt! Du musst einen Aranna-Sternstein finden. Vielleicht weiß Tante Luna Rat... Aber sie ist auf Beteigeuze.";
    }
    [0x0000]
    {
        from = "Oh no, you went into the Interstellar Hub without the right starstone to get back home! You need to find an Aranna Starstone.";
        to = "Oh nein, du bist in den Interstellaren Hub geflogen ohne den richtigen Sternstein der dich zurückbringt! Du musst einen Aranna-Sternstein finden.";
    }
    [0x0000]
