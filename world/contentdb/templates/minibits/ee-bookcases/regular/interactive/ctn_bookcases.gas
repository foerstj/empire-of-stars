[t:template,n:bookcase_glb_gen] { specializes = bookcase_glb_gen_base; }
[t:template,n:bookcase_glb_gen_base]  // bookcase_glb_gen_01 with dummy lore book
{
	specializes	= prop;
	category_name =	"container";
	doc	= "bookcase for lorebooks";

	[aspect]
	{
		model = m_i_glb_bookcase-01;
		lodfi_lower = -1;
		lodfi_upper = -1;
		does_block_path = true;
	}
	[generator_object_pcontent]
	{
		create_msg = WE_REQ_USE;
		one_shot = true;
	}
	[common]
	{
		screen_name = "Bookcase";
		rollover_display = true;
	}
	[inventory]
	{
	}
}

