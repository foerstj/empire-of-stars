[t:template,n:eos_ring_armor]
{
	specializes = ring;
	doc = "eos armor ring";
	common:screen_name	= "Rough Ring of Stars";
    common:is_pcontent_allowed = false;
    gui:inventory_icon = b_gui_ig_i_it_ring_005;
    aspect:gold_value		= 220;

	[magic]
	{
		[enchantments]
		{
			[*]
			{
				alteration			= alter_ARMOR;
				value				= 3;
				description			= "Adds 3 to Armor";
				duration			= #infinite;
				is_permanent		= true;
				is_single_instance	= false;
			}
		}
	}
}
