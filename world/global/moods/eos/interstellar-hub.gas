[mood_setting*]
{
	mood_name				= eos_interstellar_hub_teleport;
	transition_time			= 40.0f;
	interior				= true;

	[frustum]
	{
		frustum_width		= 60.0f;
		frustum_height		= 60.0f;
	}
	
	[fog]
	{
		fog_color			= 0xFF000000;
		fog_density			= 0.5f;
		fog_near_dist		= 50.0f;
		fog_far_dist		= 60.0f;
		fog_lowdetail_near_dist		= 50.0f;
		fog_lowdetail_far_dist		= 55.0f;
	}
	[rain] {}

	[music]
	{
		ambient_track		= "";
		standard_track		= "s_m_Farmhouse_04";
		room_type			= rt_parkinglot;
	}
}

[mood_setting*]  // all going dark while flying up. re-defining multiplayer_world_teleport here for me because I don't like what SeeFar does with it
{
	mood_name				= eos_teleport;
	transition_time			= 2.0f;
	interior				= true;

	[frustum]
	{
		frustum_width		= 5.0f;
		frustum_height		= 60.0f;
	}

	[fog]
	{
		fog_color			= 0xFF000000;
		fog_density			= 0.5f;
		fog_near_dist		= 20.0f;
		fog_far_dist		= 25.0f;
		fog_lowdetail_near_dist		= 15.0f;
		fog_lowdetail_far_dist		= 20.0f;
	}
	[rain] {}

	[music]
	{
		ambient_track		= "";
		room_type			= rt_generic;
	}
}
