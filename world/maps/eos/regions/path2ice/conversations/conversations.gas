[aunt_luna]
{
	[text*]
	{
		activate_quest* = find_way_home,1;
		choice = more;
		complete_quest* = find_aunt_luna;
	  i order = 0;
		screen_text = "What took you so long?";
	  f scroll_rate = 10.000000;
	}
	[text*]
	{
		choice = more;
	  i order = 1;
		screen_text = "I'm actually so happy to see you, my dearest!
See what I discovered - the ancient Empire of Stars had contact with Beings from other actual stars! Extraterrestrials! It was an interstellar empire spanning across a whole eight star systems! It's truly incredible!";
	  f scroll_rate = 10.000000;
	}
	[text*]
	{
		choice = potential_member;
		screen_text = "What do you mean, you want to go HOME of all places?
...Alright let's go then... But we need to find an Aranna Starstone first...";
	  f scroll_rate = 10.000000;
	}
}
