	}
	[text*]
	{
		choice = more;
	  i order = 1;
		screen_text = "I'm actually so happy to see you, my dearest!
See what I discovered here - the ancient Empire of Stars had contact with Beings from other actual stars! Extraterrestrials! It was an interstellar empire spanning across a whole eight star systems! It's truly incredible!";
	  f scroll_rate = 10.000000;
	}
	[text*]
